@prefix : <http://isagog.com/ontologies/pkg#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://isagog.com/ontologies/pkg> .

<http://isagog.com/ontologies/pkg> rdf:type owl:Ontology ;
                                    dc:creator "Guido Vetere" ;
                                    dcterms:license "CC-NC-BY" ;
                                    rdfs:comment "A conceptual model for Personal Knowledge Graphs" ,
                                                 "This conceptual model adopts a reductionist approach: the set of entities required to account for elements in the domain of discourse is minimal. For instance, a relationship (the connection of two persons) can also be an event, insofar the representation of its temporal properties is needed. This way, there is no need of having an instance for the reified relation (continuant) and another one for its temporal aspects (occurrent). As a consequence, there's no disjointess at the top level, e.g. between continuants and occurrents." ;
                                    rdfs:label "Personal Knowledge Graph Ontology" ;
                                    owl:versionInfo 0.1 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://isagog.com/ontologies/pkg#element
:element rdf:type owl:ObjectProperty ;
         rdfs:domain :Collection ;
         rdfs:range :Person .


###  http://isagog.com/ontologies/pkg#elementOf
:elementOf rdf:type owl:ObjectProperty .


###  http://isagog.com/ontologies/pkg#involvedIn
:involvedIn rdf:type owl:ObjectProperty ;
            owl:inverseOf :involves ;
            rdfs:domain :Entity ;
            rdfs:range :Event .


###  http://isagog.com/ontologies/pkg#involves
:involves rdf:type owl:ObjectProperty ;
          rdfs:domain :Entity ;
          rdfs:range :Event .


###  http://isagog.com/ontologies/pkg#member
:member rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :element ;
        rdfs:domain :Organization .


###  http://isagog.com/ontologies/pkg#memberOf
:memberOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :elementOf .


###  http://isagog.com/ontologies/pkg#reference
:reference rdf:type owl:ObjectProperty ;
           rdfs:domain :Information ;
           rdfs:range :Concept .


###  http://isagog.com/ontologies/pkg#repliedBy
:repliedBy rdf:type owl:ObjectProperty ;
           rdfs:domain :Message ;
           rdfs:range :Message .


###  http://isagog.com/ontologies/pkg#reply
:reply rdf:type owl:ObjectProperty ;
       rdfs:domain :Message ;
       rdfs:range :Message .


###  http://isagog.com/ontologies/pkg#topic
:topic rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf owl:topObjectProperty ;
       rdfs:domain :Information ;
       rdfs:range :Concept .


#################################################################
#    Data properties
#################################################################

###  http://isagog.com/ontologies/pkg#eventDuration
:eventDuration rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :timeSpan .


###  http://isagog.com/ontologies/pkg#eventOccurrence
:eventOccurrence rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :timeInstant .


###  http://isagog.com/ontologies/pkg#familyName
:familyName rdf:type owl:DatatypeProperty ;
            rdfs:domain :Person ;
            rdfs:range rdfs:Literal .


###  http://isagog.com/ontologies/pkg#firstName
:firstName rdf:type owl:DatatypeProperty ;
           rdfs:domain :Person ;
           rdfs:range rdfs:Literal .


###  http://isagog.com/ontologies/pkg#mention
:mention rdf:type owl:DatatypeProperty ;
         rdfs:domain :Information ;
         rdfs:range xsd:anyURI .


###  http://isagog.com/ontologies/pkg#text
:text rdf:type owl:DatatypeProperty ;
      rdfs:domain :TextualEntity ;
      rdfs:range xsd:string .


###  http://isagog.com/ontologies/pkg#timeInstant
:timeInstant rdf:type owl:DatatypeProperty ;
             rdfs:domain :Event ;
             rdfs:range xsd:dateTime .


###  http://isagog.com/ontologies/pkg#timeSpan
:timeSpan rdf:type owl:DatatypeProperty ;
          rdfs:domain :Event ;
          rdfs:range xsd:long .


#################################################################
#    Classes
#################################################################

###  http://isagog.com/ontologies/pkg#Activity
:Activity rdf:type owl:Class ;
          rdfs:subClassOf :Event .


###  http://isagog.com/ontologies/pkg#Agent
:Agent rdf:type owl:Class ;
       rdfs:subClassOf :Entity ;
       rdfs:comment "An entity capable of initiating actions" .


###  http://isagog.com/ontologies/pkg#Collection
:Collection rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :element ;
                              owl:someValuesFrom :Concept
                            ] ;
            rdfs:comment "Any collective thing which is cognitively and socially recognizable" ;
            rdfs:seeAlso <http://www.w3.org/2004/02/skos/core#Collection> .


###  http://isagog.com/ontologies/pkg#Concept
:Concept rdf:type owl:Class ;
         rdfs:comment "Any singular thing which is cognitively and socially recognizable" ;
         rdfs:seeAlso <http://www.w3.org/2004/02/skos/core#Concept> .


###  http://isagog.com/ontologies/pkg#Conversation
:Conversation rdf:type owl:Class ;
              rdfs:subClassOf :Event ,
                              :TextualStructure ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :element ;
                                owl:allValuesFrom :Message
                              ] .


###  http://isagog.com/ontologies/pkg#Document
:Document rdf:type owl:Class ;
          rdfs:subClassOf :TextualStructure .


###  http://isagog.com/ontologies/pkg#Email
:Email rdf:type owl:Class ;
       rdfs:subClassOf :TextualStructure ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :element ;
                         owl:allValuesFrom :Passage
                       ] .


###  http://isagog.com/ontologies/pkg#Entity
:Entity rdf:type owl:Class ;
        rdfs:subClassOf :Concept ;
        rdfs:comment "Any concrete or social object, abstractly from the temporal aspects" .


###  http://isagog.com/ontologies/pkg#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :Concept ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :eventOccurrence ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] ;
       rdfs:comment "Anything with direct temporal properties (start, duration)" .


###  http://isagog.com/ontologies/pkg#Information
:Information rdf:type owl:Class ;
             rdfs:subClassOf :Entity ;
             rdfs:comment "Informative entity" .


###  http://isagog.com/ontologies/pkg#Interaction
:Interaction rdf:type owl:Class ;
             rdfs:subClassOf :Event ,
                             :Relationship ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :element ;
                               owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                               owl:onClass :Person
                             ] ;
             rdfs:comment "Any event of human interaction" .


###  http://isagog.com/ontologies/pkg#Message
:Message rdf:type owl:Class ;
         rdfs:subClassOf :TextualElement ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :elementOf ;
                           owl:allValuesFrom :Conversation
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :reply ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Message
                         ] .


###  http://isagog.com/ontologies/pkg#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf :Agent ,
                              :Relationship ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :member ;
                                owl:someValuesFrom :Agent
                              ] ;
              rdfs:comment "An agentive human collective" .


###  http://isagog.com/ontologies/pkg#Passage
:Passage rdf:type owl:Class ;
         rdfs:subClassOf :TextualElement .


###  http://isagog.com/ontologies/pkg#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf :Agent ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :familyName ;
                          owl:someValuesFrom rdfs:Literal
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :firstName ;
                          owl:someValuesFrom rdfs:Literal
                        ] .


###  http://isagog.com/ontologies/pkg#Place
:Place rdf:type owl:Class ;
       rdfs:subClassOf :Entity .


###  http://isagog.com/ontologies/pkg#Relationship
:Relationship rdf:type owl:Class ;
              rdfs:subClassOf :Collection ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :element ;
                                owl:allValuesFrom :Person
                              ] ;
              rdfs:comment "Personal mutual relationship (e.g. club membership)" .


###  http://isagog.com/ontologies/pkg#TextualElement
:TextualElement rdf:type owl:Class ;
                rdfs:subClassOf :TextualEntity ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :elementOf ;
                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :TextualStructure
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :text ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:string
                                ] .


###  http://isagog.com/ontologies/pkg#TextualEntity
:TextualEntity rdf:type owl:Class ;
               rdfs:subClassOf :Information ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :text ;
                                 owl:someValuesFrom xsd:string
                               ] .


###  http://isagog.com/ontologies/pkg#TextualStructure
:TextualStructure rdf:type owl:Class ;
                  rdfs:subClassOf :TextualEntity ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :element ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :TextualElement
                                  ] .


###  http://isagog.com/ontologies/pkg#VisualEntity
:VisualEntity rdf:type owl:Class ;
              rdfs:subClassOf :Information .


