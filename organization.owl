###  http://isagog.com/ontologies/organization.owl#filler
:filler rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#initiator> .


###  http://isagog.com/ontologies/organization.owl#member-of-federation
:member-of-federation rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :organizationally-related ;
                      rdfs:range :FederativeMembership .


###  http://isagog.com/ontologies/organization.owl#membership
:membership rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :organizationally-related ;
            rdf:type owl:SymmetricProperty ;
            rdfs:range :Membership .


###  http://isagog.com/ontologies/organization.owl#organizationally-related
:organizationally-related rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#logically-related> .


###  http://isagog.com/ontologies/organization.owl#participated
:participated rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#logically-related> ;
              rdfs:range :Organization .


###  http://isagog.com/ontologies/organization.owl#role
:role rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#type> ;
      rdfs:range :OrganizationRole .


###  http://isagog.com/ontologies/organization.owl#role-feature
:role-feature rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#abstract-part> ;
              rdfs:domain :Organization ;
              rdfs:range :OrganizationRole .


###  http://isagog.com/ontologies/organization.owl#sub-organization
:sub-organization rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#element> ;
                  owl:inverseOf :super-organization .


###  http://isagog.com/ontologies/organization.owl#super-organization
:super-organization rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#collector> .


