#################################################################
#    Annotation properties
#################################################################

###  http://isagog.com/ontologies/infosystems.owl#attribute-type
:attribute-type rdf:type owl:AnnotationProperty ;
                rdfs:range :AttributeType .


###  http://www.w3.org/2002/07/owl#maxQualifiedCardinality
owl:maxQualifiedCardinality rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://isagog.com/ontologies/infosystems.owl#access
:access rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#informative-part> ;
        rdfs:domain :QuerySlot ;
        rdfs:range :AccessRole .


###  http://isagog.com/ontologies/infosystems.owl#bind-attribute
:bind-attribute rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#binding> ;
                rdfs:domain :QuerySlot ;
                owl:propertyDisjointWith :bind-property .


###  http://isagog.com/ontologies/infosystems.owl#bind-concept
:bind-concept rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#binding> ;
              rdfs:domain :QuerySlot .


###  http://isagog.com/ontologies/infosystems.owl#bind-property
:bind-property rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#binding> ;
               rdfs:domain :QuerySlot .


#################################################################
#    Data properties
#################################################################

###  http://isagog.com/ontologies/infosystems.owl#distinguished
:distinguished rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#data-value> ;
               rdfs:domain :QuerySlot ;
               rdfs:range xsd:boolean .


###  http://isagog.com/ontologies/infosystems.owl#object-topic
:object-topic rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#slot-reference> .


###  http://isagog.com/ontologies/infosystems.owl#required
:required rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#property> ;
          rdfs:domain :QuerySlot ;
          rdfs:range xsd:boolean .


###  http://isagog.com/ontologies/infosystems.owl#subject-topic
:subject-topic rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#slot-reference> .


###  http://isagog.com/ontologies/infosystems.owl#topic-id
:topic-id rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#slot-id> ;
          rdfs:domain <http://isagog.com/ontologies/descriptions.owl#Topic> .


#################################################################
#    Classes
#################################################################

###  http://isagog.com/ontologies/infosystems.owl#AccessRole
:AccessRole rdf:type owl:Class ;
            rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Denotator> .


###  http://isagog.com/ontologies/infosystems.owl#AttributeType
:AttributeType rdf:type owl:Class ;
               rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Denotator> .


###  http://isagog.com/ontologies/infosystems.owl#Equals
:Equals rdf:type owl:Class ;
        rdfs:subClassOf <http://isagog.com/ontologies/descriptions.owl#Feature> ,
                        :QuerySlot .


###  http://isagog.com/ontologies/infosystems.owl#Greater
:Greater rdf:type owl:Class ;
         rdfs:subClassOf <http://isagog.com/ontologies/descriptions.owl#Feature> ,
                         :QuerySlot .


###  http://isagog.com/ontologies/infosystems.owl#Lesser
:Lesser rdf:type owl:Class ;
        rdfs:subClassOf <http://isagog.com/ontologies/descriptions.owl#Feature> ,
                        :QuerySlot .


###  http://isagog.com/ontologies/infosystems.owl#Like
:Like rdf:type owl:Class ;
      rdfs:subClassOf <http://isagog.com/ontologies/descriptions.owl#Feature> ,
                      :QuerySlot .


###  http://isagog.com/ontologies/infosystems.owl#QueryFrame
:QueryFrame rdf:type owl:Class ;
            rdfs:subClassOf <http://isagog.com/ontologies/descriptions.owl#Frame> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://isagog.com/ontologies/descriptions.owl#slot> ;
                              owl:allValuesFrom :QuerySlot
                            ] .


###  http://isagog.com/ontologies/infosystems.owl#QuerySlot
:QuerySlot rdf:type owl:Class ;
           rdfs:subClassOf <http://isagog.com/ontologies/descriptions.owl#Slot> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://isagog.com/ontologies/descriptions.owl#frame> ;
                             owl:allValuesFrom :QueryFrame
                           ] .


###  http://isagog.com/ontologies/infosystems.owl#QueryType
:QueryType rdf:type owl:Class ;
           rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Denotator> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://isagog.com/ontologies/top.owl#denoted> ;
                             owl:allValuesFrom :QueryFrame
                           ] .


###  http://isagog.com/ontologies/infosystems.owl#Value
:Value rdf:type owl:Class ;
       rdfs:subClassOf <http://isagog.com/ontologies/descriptions.owl#Feature> ,
                       :QuerySlot .


