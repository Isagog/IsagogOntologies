Class: <http://isagog.com/ontologies/civil_registry#Citizen>

    Annotations: 
        rdfs:comment "A Human as being recognized in a civil registry, i.e. with an id, a anagraphic data, etc"
    
    SubClassOf: 
        <http://isagog.com/ontologies/top.owl#Denotator>,
        <http://isagog.com/ontologies/civil_registry#citizenship> min 1 <http://isagog.com/ontologies/places#CountryName>,
        <http://isagog.com/ontologies/civil_registry#date-of-birth> exactly 1 <http://isagog.com/ontologies/time.owl#Day>,
        <http://isagog.com/ontologies/civil_registry#place-of-birth> exactly 1 <http://isagog.com/ontologies/places#CountryName>,
        <http://isagog.com/ontologies/top.owl#denoted> exactly 1 <http://isagog.com/ontologies/top.owl#Human>,
        <http://isagog.com/ontologies/civil_registry#given-name> min 1 xsd:string,
        <http://isagog.com/ontologies/civil_registry#given-surname> min 1 xsd:string,
        <http://isagog.com/ontologies/civil_registry#personal-id> exactly 1 xsd:string
    
    
Class: <http://isagog.com/ontologies/civil_registry#Citizenship>

    SubClassOf: 
        <http://isagog.com/ontologies/civil_registry#StateOfCitizen>,
        <http://isagog.com/ontologies/civil_registry#citizenship> some <http://isagog.com/ontologies/places#CountryName>
    
    
Class: <http://isagog.com/ontologies/civil_registry#LegalState>

    SubClassOf: 
        <http://isagog.com/ontologies/civil_registry#StateOfCitizen>
    
    
Class: <http://isagog.com/ontologies/civil_registry#MarriageState>

    SubClassOf: 
        <http://isagog.com/ontologies/civil_registry#StateOfCitizen>
    
    
Class: <http://isagog.com/ontologies/civil_registry#StateOfCitizen>

    SubClassOf: 
        <http://isagog.com/ontologies/top.owl#State>,
        <http://isagog.com/ontologies/top.owl#carrier> only <http://isagog.com/ontologies/civil_registry#Citizen>
    
    
Class: <http://isagog.com/ontologies/places#CityName>

    
Class: <http://isagog.com/ontologies/places#CountryName>

    
Class: <http://isagog.com/ontologies/places#LocationReference>

    
