@prefix : <http://isagog.com/ontologies/ckg#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<http://isagog.com/ontologies/ckg> a owl:Ontology ;
	owl:versionIRI <http://isagog.com/ontologies/ckg/1.0.0> ;
	dc:creator "Guido Vetere" ;
	dcterms:license "CC-BY-SA, Isagog Srl" ;
	rdfs:comment "An upper ontology for Customizable Knowledge Graphs" , "This ontology adopts a 'deflationary' approach: the set of entities required to account for elements in the domain of discourse is minimal. For instance, a relationship (e.g. the connection of two persons) can also be an event, insofar the representation of its temporal properties is required. This way, there is no need of having an instance for the reified relation (continuant) and another one for its temporal aspects (occurrent). This implies that there's no disjointess at the level of basic categories such as 'continuants' and 'occurrents'. The resulting conceptualization trades metaphysics for natural language, by providing an easy accommodation for regular polysemy. At the cost of weakening the ontological import of the conceptualization, this is expected to improve linguistic usability." ;
	rdfs:label "Custom Knowledge Graph Ontology" .
# 
# 
# #################################################################
# #
# #    Annotation properties
# #
# #################################################################
# 
# 
# http://isagog.com/ontologies/ckg#DOLCE

:DOLCE a owl:AnnotationProperty .
# 
# http://isagog.com/ontologies/ckg#lexicalization

:lexicalization a owl:AnnotationProperty .
# 
# http://isagog.com/ontologies/ckg#permission

:permission a owl:AnnotationProperty ;
	rdfs:comment "Access control token" ;
	rdfs:range xsd:NMTOKEN .
# 
# http://isagog.com/ontologies/ckg#provenance

:provenance a owl:AnnotationProperty ;
	rdfs:comment "Provenance (URI)" ;
	rdfs:range xsd:anyURI .
# 
# http://isagog.com/ontologies/ckg#schema

:schema a owl:AnnotationProperty .
# 
# http://isagog.com/ontologies/ckg#wordObject

:wordObject a owl:AnnotationProperty ;
	rdfs:subPropertyOf :lexicalization ;
	rdfs:range :Word .
# 
# http://isagog.com/ontologies/ckg#wordString

:wordString a owl:AnnotationProperty ;
	rdfs:subPropertyOf :lexicalization ;
	rdfs:range xsd:string .
# 
# http://isagog.com/ontologies/ckg#wordnetUri

:wordnetUri a owl:AnnotationProperty ;
	rdfs:subPropertyOf :lexicalization ;
	rdfs:range xsd:anyURI .
# 
# http://purl.org/dc/elements/1.1/creator

dc:creator a owl:AnnotationProperty .
# 
# http://purl.org/dc/terms/license

dcterms:license a owl:AnnotationProperty .
# 
# http://www.w3.org/2002/07/owl#minQualifiedCardinality

owl:minQualifiedCardinality a owl:AnnotationProperty .
# 
# http://www.w3.org/2002/07/owl#qualifiedCardinality

owl:qualifiedCardinality a owl:AnnotationProperty .
# 
# 
# 
# #################################################################
# #
# #    Object Properties
# #
# #################################################################
# 
# 
# http://isagog.com/ontologies/ckg#dependsOn

:dependsOn a owl:ObjectProperty ;
	owl:inverseOf :hasDependent ;
	rdfs:domain :Entity ;
	rdfs:range :Entity ;
	rdfs:comment "Existential dependency" .
# 
# http://isagog.com/ontologies/ckg#elementOf

:elementOf a owl:ObjectProperty ;
	rdfs:subPropertyOf :partOf ;
	owl:inverseOf :hasElement ;
	rdfs:domain :Entity ;
	rdfs:range :Collection .
# 
# http://isagog.com/ontologies/ckg#hasDependent

:hasDependent a owl:ObjectProperty .
# 
# http://isagog.com/ontologies/ckg#hasElement

:hasElement a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasPart ;
	rdfs:domain :Collection ;
	rdfs:range :Entity ;
	rdfs:comment "Relationships have entities as elements" .
# 
# http://isagog.com/ontologies/ckg#hasPart

:hasPart a owl:ObjectProperty ;
	owl:inverseOf :partOf .
# 
# http://isagog.com/ontologies/ckg#hasSource

:hasSource a owl:ObjectProperty ;
	rdfs:domain :Information ;
	rdfs:range :InformationSource ;
	rdfs:comment "Information comes to some source" .
# 
# http://isagog.com/ontologies/ckg#involvedIn

:involvedIn a owl:ObjectProperty ;
	owl:inverseOf :involves ;
	rdfs:domain :Continuant ;
	rdfs:range :Occurrent ;
	rdfs:comment "Continuants (objects) involved in occurrents (events)" .
# 
# http://isagog.com/ontologies/ckg#involves

:involves a owl:ObjectProperty ;
	rdfs:domain :Occurrent ;
	rdfs:range :Continuant .
# 
# http://isagog.com/ontologies/ckg#locatedIn

:locatedIn a owl:ObjectProperty ;
	rdfs:range :Place .
# 
# http://isagog.com/ontologies/ckg#member

:member a owl:ObjectProperty ;
	rdfs:subPropertyOf :participant ;
	rdfs:domain :Organization ;
	rdfs:range :Person .
# 
# http://isagog.com/ontologies/ckg#memberOf

:memberOf a owl:ObjectProperty ;
	rdfs:subPropertyOf :elementOf .
# 
# http://isagog.com/ontologies/ckg#objectBinding

:objectBinding a owl:ObjectProperty ;
	rdfs:subPropertyOf :dependsOn ;
	rdfs:domain :RelationalSlot ;
	rdfs:range :RelationalSlot ;
	rdfs:comment """Slot's object binding, must be some relational slot in the same frame.
 The binding means 'same object as'.""" .
# 
# http://isagog.com/ontologies/ckg#occurrentIn

:occurrentIn a owl:ObjectProperty ;
	rdfs:domain :Occurrent ;
	rdfs:range :Period .
# 
# http://isagog.com/ontologies/ckg#partOf

:partOf a owl:ObjectProperty .
# 
# http://isagog.com/ontologies/ckg#participant

:participant a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasElement ;
	rdfs:domain :Collection ;
	rdfs:range :Person .
# 
# http://isagog.com/ontologies/ckg#references

:references a owl:ObjectProperty ;
	owl:inverseOf :referredBy ;
	rdfs:domain :Information ;
	rdfs:range _:genid1 .

_:genid1 a owl:Class ;
	owl:unionOf _:genid3 .

_:genid3 a rdf:List ;
	rdf:first :Collection ;
	rdf:rest _:genid2 .

_:genid2 a rdf:List ;
	rdf:first :Entity ;
	rdf:rest rdf:nil .

:references rdfs:comment "Information entities refer to other entities or relationships" .
# 
# http://isagog.com/ontologies/ckg#referredBy

:referredBy a owl:ObjectProperty .
# 
# http://isagog.com/ontologies/ckg#subjectBinding

:subjectBinding a owl:ObjectProperty ;
	rdfs:subPropertyOf :dependsOn ;
	rdfs:domain _:genid4 .

_:genid4 a owl:Class ;
	owl:unionOf _:genid6 .

_:genid6 a rdf:List ;
	rdf:first :AttributiveSlot ;
	rdf:rest _:genid5 .

_:genid5 a rdf:List ;
	rdf:first :RelationalSlot ;
	rdf:rest rdf:nil .

:subjectBinding rdfs:range :Slot ;
	rdfs:comment """Slot's subject binding, must be some slot in the same frame.
If the bound slot is a relational one, the binding means 'same subject as'.""" .
# 
