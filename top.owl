@prefix : <http://isagog.com/ontologies/top.owl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix obda: <https://w3id.org/obda/vocabulary#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://isagog.com/ontologies/top.owl> a owl:Ontology ;
	owl:versionIRI <http://isagog.com/ontologies/top.owl/1.2> ;
	:status "UNDER DEVELOPMENT" ;
	dc:creator "Guido Vetere" ;
	dct:license """https://creativecommons.org/licenses/by-sa/4.0/
by isagog.com""" ;
	rdfs:comment "An introductory upper-level ontology. At the basis of this ontology there are few primitive formal and material notions, including space, time, parthood, dependency, and agentivity."@en ;
	rdfs:label "Isagog Upper Ontology" .
# 
# 
# #################################################################
# #
# #    Annotation properties
# #
# #################################################################
# 
# 
# http://isagog.com/ontologies/top.owl#authors

:authors a owl:AnnotationProperty ;
	:authors "Guido Vetere" .
# 
# http://isagog.com/ontologies/top.owl#category

:category a owl:AnnotationProperty ;
	rdfs:comment "Ontological commitment of linguistic senses"@en ;
	rdfs:subPropertyOf :ontological-annotation ;
	rdfs:range xsd:anyURI .
# 
# http://isagog.com/ontologies/top.owl#copyright

:copyright a owl:AnnotationProperty ;
	:authors "(CC-BY) Isagog S.r.l." .
# 
# http://isagog.com/ontologies/top.owl#definition

:definition a owl:AnnotationProperty ;
	rdfs:subPropertyOf :ontological-annotation .
# 
# http://isagog.com/ontologies/top.owl#ontological-annotation

:ontological-annotation a owl:AnnotationProperty .
# 
# http://isagog.com/ontologies/top.owl#status

:status a owl:AnnotationProperty ;
	rdfs:comment "The state of development of a resource (e.g. COMPLETE, DRAFT)" .
# 
# http://isagog.com/ontologies/top.owl#support

:support a owl:AnnotationProperty ;
	rdfs:comment "Possibilistic support for membership assertions in the interval (0,1)"@en ;
	rdfs:range xsd:double .
# 
# http://purl.org/dc/elements/1.1/creator

dc:creator a owl:AnnotationProperty .
# 
# http://purl.org/dc/terms/license

dct:license a owl:AnnotationProperty .
# 
# http://www.w3.org/2000/01/rdf-schema#comment

rdfs:comment a owl:AnnotationProperty .
# 
# http://www.w3.org/2000/01/rdf-schema#isDefinedBy

rdfs:isDefinedBy a owl:AnnotationProperty .
# 
# http://www.w3.org/2000/01/rdf-schema#label

rdfs:label a owl:AnnotationProperty .
# 
# http://www.w3.org/2000/01/rdf-schema#seeAlso

rdfs:seeAlso a owl:AnnotationProperty .
# 
# http://www.w3.org/2002/07/owl#versionInfo

owl:versionInfo a owl:AnnotationProperty .
# 
# 
# 
# #################################################################
# #
# #    Datatypes
# #
# #################################################################
# 
# 
# http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral

rdf:PlainLiteral a rdfs:Datatype .
# 
# http://www.w3.org/2001/XMLSchema#anyURI

xsd:anyURI a rdfs:Datatype .
# 
# http://www.w3.org/2001/XMLSchema#string

xsd:string a rdfs:Datatype .
# 
# http://www.w3.org/2002/07/owl#real

owl:real a rdfs:Datatype .
# 
# 
# 
# #################################################################
# #
# #    Object Properties
# #
# #################################################################
# 
# 
# http://isagog.com/ontologies/top.owl#abstract-part

:abstract-part a owl:ObjectProperty ;
	rdfs:subPropertyOf :logically-related , :part ;
	owl:inverseOf :abstract-whole ;
	rdfs:domain :AbstractEntity ;
	owl:propertyDisjointWith :concrete-part ;
	rdfs:comment "Parthood among abstracta"@en .
# 
# http://isagog.com/ontologies/top.owl#abstract-whole

:abstract-whole a owl:ObjectProperty ;
	rdfs:subPropertyOf :logically-related , :whole ;
	owl:propertyDisjointWith :concrete-whole .
# 
# http://isagog.com/ontologies/top.owl#begin

:begin a owl:ObjectProperty ;
	rdfs:subPropertyOf :phase ;
	rdfs:range :Event ;
	rdfs:comment "the beginning part of an event"@en .
# 
# http://isagog.com/ontologies/top.owl#carrier

:carrier a owl:ObjectProperty ;
	rdfs:subPropertyOf :dependee , :participant ;
	rdfs:comment "The continuant the event depends upon"@en .
# 
# http://isagog.com/ontologies/top.owl#collector

:collector a owl:ObjectProperty ;
	rdfs:subPropertyOf :whole ;
	owl:inverseOf :element ;
	rdfs:domain :Continuant ;
	rdfs:range :Collection ;
	rdfs:comment "Inverse of element"@en .
# 
# http://isagog.com/ontologies/top.owl#concrete-part

:concrete-part a owl:ObjectProperty ;
	rdfs:subPropertyOf :part ;
	rdfs:comment "Parthood among physical entities"@en .
# 
# http://isagog.com/ontologies/top.owl#concrete-whole

:concrete-whole a owl:ObjectProperty ;
	rdfs:subPropertyOf :whole .
# 
# http://isagog.com/ontologies/top.owl#context

:context a owl:ObjectProperty ;
	rdfs:subPropertyOf :collector ;
	owl:inverseOf :context-element .
# 
# http://isagog.com/ontologies/top.owl#context-element

:context-element a owl:ObjectProperty ;
	rdfs:subPropertyOf :element ;
	rdfs:domain :Collection ;
	rdfs:range :Continuant ;
	rdfs:comment "Temporal element in a context"@en .
# 
# http://isagog.com/ontologies/top.owl#created

:created a owl:ObjectProperty ;
	rdfs:subPropertyOf :dependent ;
	owl:inverseOf :creator .
# 
# http://isagog.com/ontologies/top.owl#creator

:creator a owl:ObjectProperty ;
	rdfs:subPropertyOf :dependee ;
	rdfs:domain :Continuant ;
	rdfs:range :Agent .
# 
# http://isagog.com/ontologies/top.owl#denoted

:denoted a owl:ObjectProperty ;
	rdfs:subPropertyOf :logically-related ;
	owl:inverseOf :denoter ;
	rdfs:domain :AbstractEntity ;
	rdfs:comment "Generic semiotic relationship"@en .
# 
# http://isagog.com/ontologies/top.owl#denoter

:denoter a owl:ObjectProperty ;
	rdfs:subPropertyOf :logically-related ;
	rdfs:range :AbstractEntity .
# 
# http://isagog.com/ontologies/top.owl#dependee

:dependee a owl:ObjectProperty ;
	owl:inverseOf :dependent ;
	rdfs:comment "Inverse of dependent"@en .
# 
# http://isagog.com/ontologies/top.owl#dependent

:dependent a owl:ObjectProperty ;
	rdfs:comment "Existential dependency relation: the dependent entity (range) wouldn't exist without some other entity of some kind (domain) - e.g. dependent (cause,effect)"@en .
# 
# http://isagog.com/ontologies/top.owl#element

:element a owl:ObjectProperty ;
	rdfs:subPropertyOf :part ;
	rdfs:domain :Collection ;
	rdfs:range :Continuant ;
	rdfs:comment "Part relationship which does not yield dependency"@en .
# 
# http://isagog.com/ontologies/top.owl#end

:end a owl:ObjectProperty ;
	rdfs:subPropertyOf :phase ;
	rdfs:range :Event ;
	rdfs:comment "the ending part of an event"@en .
# 
# http://isagog.com/ontologies/top.owl#eventuality

:eventuality a owl:ObjectProperty ;
	rdfs:subPropertyOf :whole ;
	owl:inverseOf :participant ;
	rdfs:domain :Continuant ;
	rdfs:range :Occurrent ;
	rdfs:comment "The event role in a participation, inverse of participant, e.g. eventuality(player, match)"@en .
# 
# http://isagog.com/ontologies/top.owl#experience

:experience a owl:ObjectProperty ;
	rdfs:subPropertyOf :eventuality ;
	owl:inverseOf :experiencer .
# 
# http://isagog.com/ontologies/top.owl#experiencer

:experiencer a owl:ObjectProperty ;
	rdfs:subPropertyOf :participant ;
	rdfs:comment "Passive participant"@en .
# 
# http://isagog.com/ontologies/top.owl#generalization

:generalization a owl:ObjectProperty ;
	rdfs:subPropertyOf :logically-related ;
	owl:inverseOf :specialization ;
	rdfs:domain :Denotator ;
	rdfs:range :Denotator ;
	rdfs:comment "Informatiive generalization"@en .
# 
# http://isagog.com/ontologies/top.owl#grounding

:grounding a owl:ObjectProperty ;
	rdfs:subPropertyOf :dependee ;
	rdfs:domain :AbstractEntity ;
	rdfs:range :PhysicalEntity ;
	rdfs:comment "The spatio-temporal entity in which an abstraction grounds"@en .
# 
# http://isagog.com/ontologies/top.owl#informative-part

:informative-part a owl:ObjectProperty ;
	rdfs:subPropertyOf :logically-related , :part ;
	owl:inverseOf :informative-whole ;
	rdfs:range :Information .
# 
# http://isagog.com/ontologies/top.owl#informative-whole

:informative-whole a owl:ObjectProperty ;
	rdfs:subPropertyOf :whole .
# 
# http://isagog.com/ontologies/top.owl#initiated

:initiated a owl:ObjectProperty ;
	rdfs:subPropertyOf :eventuality ;
	owl:inverseOf :initiator .
# 
# http://isagog.com/ontologies/top.owl#initiator

:initiator a owl:ObjectProperty ;
	rdfs:subPropertyOf :participant ;
	rdfs:comment "Active participant"@en .
# 
# http://isagog.com/ontologies/top.owl#location

:location a owl:ObjectProperty ;
	rdfs:subPropertyOf :logically-related ;
	rdfs:range :Region .
# 
# http://isagog.com/ontologies/top.owl#logically-related

:logically-related a owl:ObjectProperty ;
	rdfs:subPropertyOf :related ;
	rdfs:comment "Logical relatioships, i.e. dependent on cognitive, linguistic activities"@en .
# 
# http://isagog.com/ontologies/top.owl#material-constituent

:material-constituent a owl:ObjectProperty ;
	rdfs:subPropertyOf :material-part ;
	owl:inverseOf :material-item ;
	rdfs:domain :Object ;
	rdfs:range :Substance ;
	rdfs:comment "Material constituency, e.g. this table is constituted (made) of wood"@en .
# 
# http://isagog.com/ontologies/top.owl#material-item

:material-item a owl:ObjectProperty ;
	rdfs:subPropertyOf :spatial-whole ;
	rdfs:comment "Inverse of material constituent, e.g. this wood constitutes a table"@en .
# 
# http://isagog.com/ontologies/top.owl#material-part

:material-part a owl:ObjectProperty ;
	rdfs:subPropertyOf :concrete-part ;
	owl:inverseOf :spatial-whole ;
	rdfs:domain :PhysicalEntity ;
	rdfs:range :PhysicalEntity ;
	rdfs:comment "Spatial parthood"^^xsd:string .
# 
# http://isagog.com/ontologies/top.owl#part

:part a owl:ObjectProperty ;
	owl:inverseOf :whole ;
	rdfs:comment "Composition relationship, where domain is the whole and range the part, e.g. part(bike,wheel)"@en .
# 
# http://isagog.com/ontologies/top.owl#participant

:participant a owl:ObjectProperty ;
	rdfs:subPropertyOf :concrete-part ;
	rdfs:domain :Occurrent ;
	rdfs:range :Continuant ;
	rdfs:comment "Event participation, e.g. participant(match,player)"@en .
# 
# http://isagog.com/ontologies/top.owl#phase

:phase a owl:ObjectProperty ;
	rdfs:subPropertyOf :temporal-part ;
	rdfs:comment "Ordered temporal part"@en .
# 
# http://isagog.com/ontologies/top.owl#qualified

:qualified a owl:ObjectProperty ;
	rdfs:subPropertyOf :dependee ;
	owl:inverseOf :quality ;
	rdfs:domain :Quality .
# 
# http://isagog.com/ontologies/top.owl#quality

:quality a owl:ObjectProperty ;
	rdfs:subPropertyOf :dependent ;
	rdfs:range :Quality .
# 
# http://isagog.com/ontologies/top.owl#related

:related a owl:ObjectProperty ;
	rdfs:comment "Generic relationship"@en .
# 
# http://isagog.com/ontologies/top.owl#sign

:sign a owl:ObjectProperty ;
	rdfs:subPropertyOf :denoter ;
	owl:inverseOf :signified ;
	rdfs:range :Sign ;
	rdfs:comment "Signification relationship (see inverse). e.g. object sign name"@en .
# 
# http://isagog.com/ontologies/top.owl#signified

:signified a owl:ObjectProperty ;
	rdfs:subPropertyOf :denoted ;
	rdfs:domain :Sign ;
	rdfs:comment "Relationships which brings toghether social abstractions and entities of any kind (including abstractions). The domain entity (a social object) stands for the range one 'under some aspect or capacity' within semiotic processes, e.g. linguistic."@en .
# 
# http://isagog.com/ontologies/top.owl#signifier

:signifier a owl:ObjectProperty ;
	rdfs:subPropertyOf :logically-related ;
	rdfs:domain :Sign ;
	rdfs:comment "The relationship between a sign and its concrete realization"@en .
# 
# http://isagog.com/ontologies/top.owl#space-location

:space-location a owl:ObjectProperty ;
	rdfs:subPropertyOf :location ;
	rdfs:range :SpaceRegion .
# 
# http://isagog.com/ontologies/top.owl#spatial-whole

:spatial-whole a owl:ObjectProperty ;
	rdfs:subPropertyOf :concrete-whole .
# 
# http://isagog.com/ontologies/top.owl#spatially-related

:spatially-related a owl:ObjectProperty ;
	rdfs:subPropertyOf :related ;
	rdfs:comment "Spatial proximity"@en .
# 
# http://isagog.com/ontologies/top.owl#specialization

:specialization a owl:ObjectProperty ;
	rdfs:subPropertyOf :logically-related ;
	rdfs:domain :Denotator ;
	rdfs:range :Denotator ;
	rdfs:comment "Informative specialization"@en .
# 
# http://isagog.com/ontologies/top.owl#state-experience

:state-experience a owl:ObjectProperty ;
	rdfs:subPropertyOf :experience ;
	rdfs:range :State .
# 
# http://isagog.com/ontologies/top.owl#subregion

:subregion a owl:ObjectProperty ;
	rdfs:subPropertyOf :abstract-part ;
	rdfs:domain :Region .
# 
# http://isagog.com/ontologies/top.owl#temporal-constituent

:temporal-constituent a owl:ObjectProperty ;
	rdfs:subPropertyOf :temporal-part ;
	owl:inverseOf :temporal-ensemble ;
	rdfs:domain :Event ;
	rdfs:range :Event ;
	rdfs:comment "Temporal constituency, e.g. the violin play in a quartet"@en .
# 
# http://isagog.com/ontologies/top.owl#temporal-ensemble

:temporal-ensemble a owl:ObjectProperty ;
	rdfs:subPropertyOf :temporal-whole ;
	rdfs:domain :Event ;
	rdfs:range :Event ;
	rdfs:comment "Inverse of temporal-constituent"@en .
# 
# http://isagog.com/ontologies/top.owl#temporal-part

:temporal-part a owl:ObjectProperty ;
	rdfs:subPropertyOf :concrete-part ;
	owl:inverseOf :temporal-whole ;
	rdfs:domain :Event ;
	rdfs:range :Event ;
	rdfs:comment "Temporal parthood"^^xsd:string , "Temporal whole"^^xsd:string .
# 
# http://isagog.com/ontologies/top.owl#temporal-whole

:temporal-whole a owl:ObjectProperty ;
	rdfs:subPropertyOf :concrete-whole .
# 
# http://isagog.com/ontologies/top.owl#temporally-related

:temporally-related a owl:ObjectProperty ;
	rdfs:subPropertyOf :related ;
	rdfs:domain :Occurrent ;
	rdfs:range :Occurrent ;
	rdfs:comment "Temporal proximity"@en .
# 
# http://isagog.com/ontologies/top.owl#time-location

:time-location a owl:ObjectProperty ;
	rdfs:subPropertyOf :location ;
	rdfs:range :TimeRegion .
# 
# http://isagog.com/ontologies/top.owl#time-subregion

:time-subregion a owl:ObjectProperty ;
	rdfs:subPropertyOf :subregion ;
	rdfs:domain :TimeRegion ;
	rdfs:range :TimeRegion .
# 
# http://isagog.com/ontologies/top.owl#type

:type a owl:ObjectProperty ;
	rdfs:subPropertyOf :denoter ;
	rdfs:range :Denotator ;
	rdfs:comment "Typological characterization"@en .
# 
# http://isagog.com/ontologies/top.owl#whole

:whole a owl:ObjectProperty ;
	rdfs:comment "Inverse of part"@en .
# 
# http://www.w3.org/2002/07/owl#topObjectProperty

owl:topObjectProperty a owl:ObjectProperty .
# 
# 
# 
# #################################################################
# #
# #    Data properties
# #
# #################################################################
# 
# 
# http://isagog.com/ontologies/top.owl#data-value

:data-value a owl:DatatypeProperty .
# 
# http://isagog.com/ontologies/top.owl#final-time

:final-time a owl:DatatypeProperty ;
	rdfs:subPropertyOf :time-instant .
# 
# http://isagog.com/ontologies/top.owl#identifier

:identifier a owl:DatatypeProperty .
# 
# http://isagog.com/ontologies/top.owl#initial-time

:initial-time a owl:DatatypeProperty ;
	rdfs:subPropertyOf :time-instant .
# 
# http://isagog.com/ontologies/top.owl#keyword

:keyword a owl:DatatypeProperty .
# 
# http://isagog.com/ontologies/top.owl#property

:property a owl:DatatypeProperty .
# 
# http://isagog.com/ontologies/top.owl#realization

:realization a owl:DatatypeProperty ;
	rdfs:domain :Sign .
# 
# http://isagog.com/ontologies/top.owl#time-instant

:time-instant a owl:DatatypeProperty .
# 
# http://isagog.com/ontologies/top.owl#time-span

:time-span a owl:DatatypeProperty .
# 
# http://isagog.com/ontologies/top.owl#uri

:uri a owl:DatatypeProperty .
# 
# 
# 
# #################################################################
# #
# #    Classes
# #
# #################################################################
# 
# 
# http://isagog.com/ontologies/top.owl#AbstractCollection

:AbstractCollection a owl:Class ;
	rdfs:subClassOf :AbstractEntity , :Collection ;
	owl:disjointWith :PhysicalCollection ;
	rdfs:comment "Non tangible collection (no matter whether elements are physical or not)"@en .
# 
# http://isagog.com/ontologies/top.owl#AbstractEntity

:AbstractEntity a owl:Class ;
	rdfs:subClassOf :Continuant ;
	owl:disjointWith :PhysicalEntity ;
	rdfs:comment "Continuant entities not located in space (non tangible)"@en ;
	rdfs:seeAlso "DOLCE abstract" .
# 
# http://isagog.com/ontologies/top.owl#AbstractQuality

:AbstractQuality a owl:Class ;
	rdfs:subClassOf :AbstractEntity , :Quality ;
	rdfs:comment "Dependent continuants inherent in any entity"@it .
# 
# http://isagog.com/ontologies/top.owl#Action

:Action a owl:Class ;
	rdfs:subClassOf _:genid1 .

_:genid1 owl:intersectionOf _:genid4 .

_:genid4 a rdf:List ;
	rdf:first :Event ;
	rdf:rest _:genid2 .

_:genid2 a rdf:List ;
	rdf:first _:genid3 .

_:genid3 a owl:Restriction ;
	owl:onProperty :initiator ;
	owl:someValuesFrom :Agent .

_:genid2 rdf:rest rdf:nil .

_:genid1 a owl:Class .

:Action rdfs:comment "An event initiated by some agent for some purpose"@en .
# 
# http://isagog.com/ontologies/top.owl#Agent

:Agent a owl:Class ;
	rdfs:subClassOf _:genid5 .

_:genid5 owl:intersectionOf _:genid8 .

_:genid8 a rdf:List ;
	rdf:first :Object ;
	rdf:rest _:genid6 .

_:genid6 a rdf:List ;
	rdf:first _:genid7 .

_:genid7 a owl:Restriction ;
	owl:onProperty :initiated ;
	owl:someValuesFrom :Event .

_:genid6 rdf:rest rdf:nil .

_:genid5 a owl:Class .

:Agent rdfs:comment "An object capable of initiating actions"@en .
# 
# http://isagog.com/ontologies/top.owl#Aggregate

:Aggregate a owl:Class ;
	rdfs:subClassOf _:genid9 .

_:genid9 owl:intersectionOf _:genid12 .

_:genid12 a rdf:List ;
	rdf:first :PhysicalEntity ;
	rdf:rest _:genid10 .

_:genid10 a rdf:List ;
	rdf:first _:genid11 .

_:genid11 a owl:Restriction ;
	owl:onProperty :material-part ;
	owl:someValuesFrom :Object .

_:genid10 rdf:rest rdf:nil .

_:genid9 a owl:Class .

:Aggregate rdfs:comment "Continuant with elements that can be added \\ subtracted without altering the entity's identity"@en ;
	rdfs:seeAlso "BFO ObjectAggregate" .
# 
# http://isagog.com/ontologies/top.owl#Artifact

:Artifact a owl:Class ;
	rdfs:subClassOf :Object ;
	owl:disjointWith :Organism ;
	rdfs:comment "Objects created by agents"@en .
# 
# http://isagog.com/ontologies/top.owl#Change

:Change a owl:Class ;
	rdfs:subClassOf :Event , _:genid13 .

_:genid13 owl:intersectionOf _:genid16 .

_:genid16 a rdf:List ;
	rdf:first _:genid17 .

_:genid17 a owl:Restriction ;
	owl:onProperty :begin ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	owl:onClass :State .

_:genid16 rdf:rest _:genid14 .

_:genid14 a rdf:List ;
	rdf:first _:genid15 .

_:genid15 a owl:Restriction ;
	owl:onProperty :end ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	owl:onClass :State .

_:genid14 rdf:rest rdf:nil .

_:genid13 a owl:Class .

:Change rdfs:comment "Event with two distinct ordered parts (before and after), e.g. a stop"@en .
# 
# http://isagog.com/ontologies/top.owl#Collection

:Collection a owl:Class ;
	owl:equivalentClass _:genid18 .

_:genid18 a owl:Class ;
	owl:unionOf _:genid20 .

_:genid20 a rdf:List ;
	rdf:first :AbstractCollection ;
	rdf:rest _:genid19 .

_:genid19 a rdf:List ;
	rdf:first :PhysicalCollection ;
	rdf:rest rdf:nil .

:Collection rdfs:subClassOf :Continuant , _:genid21 .

_:genid21 a owl:Restriction ;
	owl:onProperty :element ;
	owl:someValuesFrom :Continuant .

:Collection rdfs:comment "Collective (i.e. with members) continuants. Collections are intensional, i.e. their identity does not necessarily depend on their members)"@en .
# 
# http://isagog.com/ontologies/top.owl#Continuant

:Continuant a owl:Class ;
	owl:equivalentClass _:genid22 .

_:genid22 a owl:Class ;
	owl:unionOf _:genid24 .

_:genid24 a rdf:List ;
	rdf:first :AbstractEntity ;
	rdf:rest _:genid23 .

_:genid23 a rdf:List ;
	rdf:first :PhysicalEntity ;
	rdf:rest rdf:nil .

:Continuant rdfs:subClassOf :Entity ;
	owl:disjointWith :Occurrent ;
	rdfs:comment "Entities fully present in every instant of their existence, e.g. a bottle, a cat, a law."@en ;
	rdfs:seeAlso "DOLCE endurant" .
# 
# http://isagog.com/ontologies/top.owl#Course

:Course a owl:Class ;
	rdfs:subClassOf :Event , _:genid25 .

_:genid25 a owl:Restriction ;
	owl:onProperty :phase ;
	owl:someValuesFrom :Event .

:Course rdfs:comment "An event with identified phases (states)"@en .
# 
# http://isagog.com/ontologies/top.owl#Denotator

:Denotator a owl:Class ;
	rdfs:subClassOf :Information , _:genid26 .

_:genid26 a owl:Restriction ;
	owl:onProperty :denoted ;
	owl:someValuesFrom :Entity .

:Denotator rdfs:comment "Social object which identifies some entity in any possible world (state of affair)."@en ;
	rdfs:seeAlso "DOLCE (ExtendedDnS) social-object" , "SKOS Concept" .
# 
# http://isagog.com/ontologies/top.owl#Entity

:Entity a owl:Class ;
	owl:equivalentClass _:genid27 .

_:genid27 a owl:Class ;
	owl:unionOf _:genid29 .

_:genid29 a rdf:List ;
	rdf:first :Continuant ;
	rdf:rest _:genid28 .

_:genid28 a rdf:List ;
