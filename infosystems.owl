@prefix : <http://isagog.com/ontologies/infosystems.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://isagog.com/ontologies/infosystems.owl> .

<http://isagog.com/ontologies/infosystems.owl> rdf:type owl:Ontology ;
                                                owl:versionIRI <http://isagog.com/ontologies/infosystems.owl/1.1.4> ;
                                                owl:imports <http://isagog.com/ontologies/descriptions.owl> ;
                                                <http://purl.org/dc/elements/1.1/creator> "Guido Vetere" ;
                                                <http://purl.org/dc/terms/license> """https://creativecommons.org/licenses/by/4.0/
Isagog S.r.l.""" ;
                                                rdfs:label "Isagog Information Systems Ontology" ;
                                                owl:versionInfo "1.1.4" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2002/07/owl#maxQualifiedCardinality
owl:maxQualifiedCardinality rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://isagog.com/ontologies/infosystems.owl#access
:access rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#informative-part> ;
        rdfs:domain :QuerySlot ;
        rdfs:range :AccessRole .


###  http://isagog.com/ontologies/infosystems.owl#bind-attribute
:bind-attribute rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#binding> ;
                rdfs:domain :QuerySlot .


###  http://isagog.com/ontologies/infosystems.owl#bind-concept
:bind-concept rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#binding> ;
              rdfs:domain :QuerySlot .


###  http://isagog.com/ontologies/infosystems.owl#bind-property
:bind-property rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#binding> ;
               rdfs:domain :QuerySlot .


#################################################################
#    Data properties
#################################################################

###  http://isagog.com/ontologies/descriptions.owl#bind-slot-object
<http://isagog.com/ontologies/descriptions.owl#bind-slot-object> rdf:type owl:DatatypeProperty ;
                                                                 rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#slot-reference> ;
                                                                 rdfs:comment "slot-id of the object binding" .


###  http://isagog.com/ontologies/descriptions.owl#bind-slot-subject
<http://isagog.com/ontologies/descriptions.owl#bind-slot-subject> rdf:type owl:DatatypeProperty ;
                                                                  rdfs:subPropertyOf <http://isagog.com/ontologies/descriptions.owl#slot-reference> ;
                                                                  rdfs:comment "slot-id of the subject bind" .


###  http://isagog.com/ontologies/infosystems.owl#distinguished
:distinguished rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#data-value> ;
               rdfs:domain :QuerySlot ;
               rdfs:range xsd:boolean .


###  http://isagog.com/ontologies/infosystems.owl#required
:required rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#property> ;
          rdfs:domain :QuerySlot ;
          rdfs:range xsd:boolean .


#################################################################
#    Classes
#################################################################

###  http://isagog.com/ontologies/infosystems.owl#AccessRole
:AccessRole rdf:type owl:Class ;
            rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Denotator> .


###  http://isagog.com/ontologies/infosystems.owl#Classification
:Classification rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :QuerySlot
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :bind-concept ;
                                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onClass <http://isagog.com/ontologies/top.owl#Classifier>
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :QuerySlot ;
                owl:disjointWith :Relation ,
                                 :Value .


###  http://isagog.com/ontologies/infosystems.owl#Equals
:Equals rdf:type owl:Class ;
        rdfs:subClassOf :Value .


###  http://isagog.com/ontologies/infosystems.owl#FreeVariable
:FreeVariable rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :QuerySlot
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :distinguished ;
                                                           owl:hasValue "true"^^xsd:boolean
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "Free variable in conjunctive query" .


###  http://isagog.com/ontologies/infosystems.owl#Greater
:Greater rdf:type owl:Class ;
         rdfs:subClassOf :Value .


###  http://isagog.com/ontologies/infosystems.owl#Lesser
:Lesser rdf:type owl:Class ;
        rdfs:subClassOf :Value .


###  http://isagog.com/ontologies/infosystems.owl#Like
:Like rdf:type owl:Class ;
      rdfs:subClassOf :Value .


###  http://isagog.com/ontologies/infosystems.owl#QueryFrame
:QueryFrame rdf:type owl:Class ;
            rdfs:subClassOf <http://isagog.com/ontologies/descriptions.owl#Frame> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://isagog.com/ontologies/descriptions.owl#slot> ;
                              owl:allValuesFrom :QuerySlot
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://isagog.com/ontologies/top.owl#type> ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :QueryType
                            ] .


###  http://isagog.com/ontologies/infosystems.owl#QuerySlot
:QuerySlot rdf:type owl:Class ;
           rdfs:subClassOf <http://isagog.com/ontologies/descriptions.owl#Slot> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://isagog.com/ontologies/descriptions.owl#binding> ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass <http://isagog.com/ontologies/top.owl#Classifier>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://isagog.com/ontologies/descriptions.owl#slot-id> ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] .


###  http://isagog.com/ontologies/infosystems.owl#QueryType
:QueryType rdf:type owl:Class ;
           rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Denotator> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://isagog.com/ontologies/top.owl#denoted> ;
                             owl:allValuesFrom :QueryFrame
                           ] .


###  http://isagog.com/ontologies/infosystems.owl#Relation
:Relation rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :QuerySlot
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :bind-property ;
                                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass <http://isagog.com/ontologies/descriptions.owl#Relationship>
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty <http://isagog.com/ontologies/descriptions.owl#bind-slot-object> ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://isagog.com/ontologies/descriptions.owl#bind-slot-subject> ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          owl:disjointWith :Value .


###  http://isagog.com/ontologies/infosystems.owl#Value
:Value rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :QuerySlot
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :bind-attribute ;
                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass <http://isagog.com/ontologies/descriptions.owl#Attribute>
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty <http://isagog.com/ontologies/top.owl#data-value> ;
                         owl:cardinality "1"^^xsd:nonNegativeInteger
                       ] .


#################################################################
#    Individuals
#################################################################

###  http://isagog.com/ontologies/infosystems.owl#ANYONE
:ANYONE rdf:type owl:NamedIndividual ,
                 :AccessRole .


###  http://isagog.com/ontologies/infosystems.owl#AUTHORIZED
:AUTHORIZED rdf:type owl:NamedIndividual ,
                     :AccessRole .


###  http://isagog.com/ontologies/infosystems.owl#AVERAGE
:AVERAGE rdf:type owl:NamedIndividual ,
                  :QueryType .


###  http://isagog.com/ontologies/infosystems.owl#COUNT
:COUNT rdf:type owl:NamedIndividual ,
                :QueryType .


###  http://isagog.com/ontologies/infosystems.owl#MAXIMUM
:MAXIMUM rdf:type owl:NamedIndividual ,
                  :QueryType .


###  http://isagog.com/ontologies/infosystems.owl#MINIMUM
:MINIMUM rdf:type owl:NamedIndividual ,
                  :QueryType .


###  http://isagog.com/ontologies/infosystems.owl#VALUES
:VALUES rdf:type owl:NamedIndividual ,
                 :QueryType .


[ owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger
] .

#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
   owl:members ( :Equals
                 :Greater
                 :Lesser
                 :Like
               )
 ] .


