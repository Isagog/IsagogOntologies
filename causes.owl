@prefix : <http://isagog.com/ontologies/causes.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix descr: <http://isagog.com/ontologies/descriptions.owl#> .
@base <http://isagog.com/ontologies/causes.owl> .

<http://isagog.com/ontologies/causes.owl> rdf:type owl:Ontology ;
                                           owl:imports <http://isagog.com/ontologies/descriptions.owl/1.2> ,
                                                       <http://isagog.com/ontologies/events.owl/1.2> ;
                                           <http://isagog.com/ontologies/top.owl#authors> "David Valente, Guido Vetere" ;
                                           <http://isagog.com/ontologies/top.owl#copyright> """https://creativecommons.org/licenses/by-sa/4.0/
by isagog.com""" ;
                                           <http://purl.org/dc/elements/1.1/creator> "Guido Vetere" ;
                                           rdfs:label "Isagog Causal Ontology" .

#################################################################
#    Object Properties
#################################################################

###  http://isagog.com/ontologies/causes.owl#confidence
:confidence rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#informative-part> ;
            rdfs:domain :Observation .


###  http://isagog.com/ontologies/causes.owl#context
:context rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://isagog.com/ontologies/events.owl#concomitant> ;
         rdf:type owl:SymmetricProperty ,
                  owl:TransitiveProperty ;
         rdfs:domain <http://isagog.com/ontologies/top.owl#Occurrent> ;
         rdfs:range <http://isagog.com/ontologies/top.owl#Occurrent> .


###  http://isagog.com/ontologies/causes.owl#evidence
:evidence rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#informative-part> ;
          rdfs:domain :Observation .


###  http://isagog.com/ontologies/causes.owl#fix
:fix rdf:type owl:ObjectProperty ;
     rdfs:subPropertyOf <http://isagog.com/ontologies/events.owl#previous> ;
     owl:inverseOf :fixed-by ;
     rdfs:domain :Procedure ;
     rdfs:range :Situation .


###  http://isagog.com/ontologies/causes.owl#fixed-by
:fixed-by rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://isagog.com/ontologies/events.owl#next> ;
          rdfs:domain :Situation ;
          rdfs:range :Procedure .


###  http://isagog.com/ontologies/causes.owl#instruction
:instruction rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#informative-part> ;
             rdfs:domain :Procedure .


###  http://isagog.com/ontologies/causes.owl#involved
:involved rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#bearer> ;
          rdfs:domain :Observation ;
          rdfs:range <http://isagog.com/ontologies/top.owl#PhysicalEntity> .


###  http://isagog.com/ontologies/causes.owl#observed
:observed rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#dependee> ;
          owl:inverseOf :observed-by ;
          rdfs:domain :Observation .


###  http://isagog.com/ontologies/causes.owl#observed-by
:observed-by rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#dependent> .


###  http://isagog.com/ontologies/causes.owl#origin
:origin rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#bearer> ;
        rdfs:domain :Situation ;
        rdfs:range <http://isagog.com/ontologies/top.owl#PhysicalEntity> .


###  http://isagog.com/ontologies/causes.owl#report
:report rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#informative-part> ;
        rdfs:domain :Observation .


#################################################################
#    Classes
#################################################################

###  http://isagog.com/ontologies/causes.owl#AutomatedTest
:AutomatedTest rdf:type owl:Class ;
               rdfs:subClassOf :Observation .


###  http://isagog.com/ontologies/causes.owl#ConfidenceMeasure
:ConfidenceMeasure rdf:type owl:Class ;
                   rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Measure> ;
                   rdfs:comment "Confidence measure (how much confident is the observer)"@en .


###  http://isagog.com/ontologies/causes.owl#EvidenceMeasure
:EvidenceMeasure rdf:type owl:Class ;
                 rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Measure> ;
                 rdfs:comment "Evidence measure (how much evidence has been gathered about the observed situation)"@en .


###  http://isagog.com/ontologies/causes.owl#HumanObservation
:HumanObservation rdf:type owl:Class ;
                  rdfs:subClassOf :Observation .


###  http://isagog.com/ontologies/causes.owl#InformationFrame
:InformationFrame rdf:type owl:Class ;
                  rdfs:subClassOf descr:Frame ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty descr:slot ;
                                    owl:someValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty descr:bind-property ;
                                                                                owl:hasValue :involved
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty descr:object-topic ;
                                                                                owl:hasValue "entity"
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty descr:slot ;
                                    owl:someValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty descr:bind-property ;
                                                                                owl:hasValue :origin
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty descr:object-topic ;
                                                                                owl:hasValue "entity"
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty descr:slot ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                         owl:onProperty descr:bind-concept ;
                                                                         owl:hasValue :PhysicalEntity
                                                                       ]
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty descr:topic-id ;
                                                                         owl:hasValue "entity"
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ]
                                  ] .


###  http://isagog.com/ontologies/causes.owl#Observation
:Observation rdf:type owl:Class ;
             rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Action> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :confidence ;
                               owl:someValuesFrom :ConfidenceMeasure
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :evidence ;
                               owl:someValuesFrom :EvidenceMeasure
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :observed ;
                               owl:someValuesFrom :Situation
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :report ;
                               owl:someValuesFrom <http://isagog.com/ontologies/top.owl#Information>
                             ] .


###  http://isagog.com/ontologies/causes.owl#ObservationFrame
:ObservationFrame rdf:type owl:Class ;
                  rdfs:subClassOf descr:Frame ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty descr:slot ;
                                    owl:someValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty descr:bind-property ;
                                                                                owl:hasValue :involved
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty descr:subject-topic ;
                                                                                owl:hasValue "obsv"
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty descr:slot ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                         owl:onProperty descr:bind-concept ;
                                                                         owl:hasValue :Observation
                                                                       ]
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty descr:topic-id ;
                                                                         owl:hasValue "obsv"
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ]
                                  ] ;
                  rdfs:comment "A statement which refers to an observation" .


###  http://isagog.com/ontologies/causes.owl#ObservedSituation
:ObservedSituation rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :Situation
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :observed-by ;
                                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                owl:onClass :Observation
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] .


###  http://isagog.com/ontologies/causes.owl#Procedure
:Procedure rdf:type owl:Class ;
           rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Course> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :instruction ;
                             owl:someValuesFrom <http://isagog.com/ontologies/top.owl#Information>
                           ] .


###  http://isagog.com/ontologies/causes.owl#Situation
:Situation rdf:type owl:Class ;
           rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Occurrent> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :context ;
                             owl:someValuesFrom :Situation
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :fixed-by ;
                             owl:someValuesFrom :Procedure
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :observed-by ;
                             owl:someValuesFrom :Observation
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://isagog.com/ontologies/events.owl#cause> ;
                             owl:someValuesFrom <http://isagog.com/ontologies/top.owl#Occurrent>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://isagog.com/ontologies/events.owl#effect> ;
                             owl:someValuesFrom <http://isagog.com/ontologies/top.owl#Occurrent>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://isagog.com/ontologies/top.owl#bearer> ;
                             owl:someValuesFrom <http://isagog.com/ontologies/top.owl#PhysicalEntity>
                           ] .


###  http://isagog.com/ontologies/descriptions.owl#ProcedureDescription
descr:ProcedureDescription rdf:type owl:Class .


###  http://isagog.com/ontologies/top.owl#Continuant
<http://isagog.com/ontologies/top.owl#Continuant> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://isagog.com/ontologies/top.owl#data-value> ;
                                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                    owl:onDataRange xsd:double
                                                                  ] .


###  http://isagog.com/ontologies/top.owl#Occurrent
<http://isagog.com/ontologies/top.owl#Occurrent> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <http://isagog.com/ontologies/top.owl#data-value> ;
                                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                   owl:onDataRange xsd:double
                                                                 ] .


#################################################################
#    Individuals
#################################################################

###  http://isagog.com/ontologies/causes.owl#Observation
:Observation rdf:type owl:NamedIndividual ,
                      descr:Classifier .


###  http://isagog.com/ontologies/causes.owl#PhysicalEntity
:PhysicalEntity rdf:type owl:NamedIndividual ,
                         descr:Classifier .


###  http://isagog.com/ontologies/causes.owl#Procedure
:Procedure rdf:type owl:NamedIndividual ,
                    descr:Classifier .


###  http://isagog.com/ontologies/causes.owl#Situation
:Situation rdf:type owl:NamedIndividual ,
                    descr:Classifier .


###  http://isagog.com/ontologies/causes.owl#involved
:involved rdf:type owl:NamedIndividual ,
                   descr:Relationship .


###  http://isagog.com/ontologies/causes.owl#origin
:origin rdf:type owl:NamedIndividual ,
                 descr:Relationship .


#################################################################
#    Annotations
#################################################################

:Observation rdfs:comment "The act of observing some state of affairs"@en .


:Procedure rdfs:comment "A course of actions"@en .


:Situation rdfs:comment "Observable event with causal relations and fixing procedure"@en .


:involved rdfs:comment "Relation to the physical entity involved in the observation"@en .


:origin rdfs:comment "Relation to the physical entity where the situation origins"@en .


