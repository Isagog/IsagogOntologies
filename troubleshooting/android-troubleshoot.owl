Individual: android-troubleshoot:MobileNetworkLatency

    Annotations: 
        top:nominal "lentezza della connessione su rete mobile"@it,
        rdfs:comment "reified class",
        top:description "la connessione sulla rete mobile è lenta"@it
  Types: 
        descriptions:Classifier
      
    
Individual: android-troubleshoot:RamExausted

    Annotations: 
