<http://isagog.com/ontologies/bibkg> rdf:type owl:Ontology ;
                                      owl:versionIRI <http://isagog.com/ontologies/bibkg/1.0.0> ;
                                      owl:imports <http://isagog.com/ontologies/ckg/1.0.0> ;
                                      rdfs:comment "Isagog bibliographic ontology" .

#################################################################
#    Object Properties
#################################################################

###  http://isagog.com/ontologies/bibkg#hasTopic
:hasTopic rdf:type owl:ObjectProperty ;
          owl:equivalentProperty dcterms:subject ;
          rdfs:subPropertyOf <http://isagog.com/ontologies/ckg#references> ;
          owl:inverseOf :isTopicOf ;
          rdfs:domain :Publication ;
          rdfs:range :Topic .


###  http://isagog.com/ontologies/bibkg#isTopicOf
:isTopicOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://isagog.com/ontologies/ckg#referredBy> .


###  http://isagog.com/ontologies/bibkg#subTopicOf
:subTopicOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://isagog.com/ontologies/ckg#partOf> ;
            owl:inverseOf :superTopicOf ;
            rdfs:domain :Topic ;
            rdfs:range :Topic .


###  http://isagog.com/ontologies/bibkg#superTopicOf
:superTopicOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://isagog.com/ontologies/ckg#hasPart> ;
              rdfs:domain :Topic ;
              rdfs:range :Topic .


###  http://purl.org/dc/terms/subject
dcterms:subject rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://isagog.com/ontologies/ckg#references> .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/author
dcterms:author rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf <http://isagog.com/ontologies/ckg#identifier> ;
               rdfs:domain :Publication .


###  http://purl.org/dc/terms/publisher
dcterms:publisher rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf <http://isagog.com/ontologies/ckg#identifier> ;
                  rdfs:domain :Publication .


#################################################################
#    Classes
#################################################################

###  http://isagog.com/ontologies/bibkg#Author
:Author rdf:type owl:Class ;
        rdfs:subClassOf <http://isagog.com/ontologies/ckg#Person> .


###  http://isagog.com/ontologies/bibkg#Book
:Book rdf:type owl:Class ;
      rdfs:subClassOf :Publication .


###  http://isagog.com/ontologies/bibkg#Journal
:Journal rdf:type owl:Class ;
         rdfs:subClassOf :Publication .


###  http://isagog.com/ontologies/bibkg#Paper
:Paper rdf:type owl:Class ;
       rdfs:subClassOf :Publication .


###  http://isagog.com/ontologies/bibkg#Proceedings
:Proceedings rdf:type owl:Class ;
             rdfs:subClassOf :Publication ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty <http://isagog.com/ontologies/ckg#hasPart> ;
                               owl:someValuesFrom :Paper
                             ] .


###  http://isagog.com/ontologies/bibkg#Publication
:Publication rdf:type owl:Class ;
             rdfs:subClassOf <http://isagog.com/ontologies/ckg#InformationSource> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasTopic ;
                               owl:someValuesFrom :Topic
                             ] .


###  http://isagog.com/ontologies/bibkg#Topic
:Topic rdf:type owl:Class ;
       rdfs:subClassOf <http://isagog.com/ontologies/ckg#Identifier> ;
       rdfs:comment "Main conceptual element of a publication, e.g. field of study" .


