<http://isagog.com/ontologies/top.owl#type> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://isagog.com/ontologies/organization.owl#VAT
:VAT rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf :organization-feature .


###  http://isagog.com/ontologies/organization.owl#legalAddress
:legalAddress rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :organization-feature .


###  http://isagog.com/ontologies/organization.owl#legalName
:legalName rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :organization-feature .


###  http://isagog.com/ontologies/organization.owl#organization-feature
:organization-feature rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf <http://isagog.com/ontologies/top.owl#data-value> ;
                      rdfs:domain :Organization .


###  http://isagog.com/ontologies/top.owl#data-value
<http://isagog.com/ontologies/top.owl#data-value> rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://isagog.com/ontologies/descriptions.owl#Procedure
<http://isagog.com/ontologies/descriptions.owl#Procedure> rdf:type owl:Class .


###  http://isagog.com/ontologies/descriptions.owl#Situation
<http://isagog.com/ontologies/descriptions.owl#Situation> rdf:type owl:Class .


###  http://isagog.com/ontologies/organization.owl#ActivityType
:ActivityType rdf:type owl:Class ;
              rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Denotator> ;
              rdfs:label "Tipo di attivita'"@it ;
              rdfs:seeAlso <https://w3id.org/italia/onto/COV#ActivityType> .


###  http://isagog.com/ontologies/organization.owl#Federation
:Federation rdf:type owl:Class ;
            rdfs:subClassOf :Organization ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :member-of-federation ;
                              owl:someValuesFrom :FederativeMembership
                            ] ;
            rdfs:comment "Federative organization" ;
            rdfs:label "Federazione"@it .


###  http://isagog.com/ontologies/organization.owl#FederativeMembership
:FederativeMembership rdf:type owl:Class ;
                      rdfs:subClassOf :OrganizationStatus ;
                      rdfs:comment "The (reified) participation of an Organization to a Federation" .


###  http://isagog.com/ontologies/organization.owl#LegalStatus
:LegalStatus rdf:type owl:Class ;
             rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Denotator> ;
             rdfs:label "Forma giuridica"@it ;
             rdfs:seeAlso <https://w3id.org/italia/onto/COV#LegalStatus> .


###  http://isagog.com/ontologies/organization.owl#Membership
:Membership rdf:type owl:Class ;
            rdfs:subClassOf :OrganizationStatus ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://isagog.com/ontologies/top.owl#participant> ;
                              owl:allValuesFrom <http://isagog.com/ontologies/top.owl#Human>
                            ] ;
            rdfs:comment "Organization membership: someone (human) participates in an organization in a certain role" .


###  http://isagog.com/ontologies/organization.owl#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf <http://isagog.com/ontologies/top.owl#AbstractCollection> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :role-feature ;
                                owl:someValuesFrom :OrganizationRole
                              ] ;
              rdfs:comment "A collective human agent ruled by a statute, with purposes, roles and responsibilities"@en ,
                           "Associazione di persone governata da uno statuto che definisce scopi, ruoli e resposabilità"@it ;
              rdfs:label "Organizzazione"@it ;
              rdfs:seeAlso <http://www.w3.org/ns/org#Organization> .


###  http://isagog.com/ontologies/organization.owl#OrganizationChange
:OrganizationChange rdf:type owl:Class ;
                    rdfs:subClassOf <http://isagog.com/ontologies/descriptions.owl#Procedure> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://isagog.com/ontologies/top.owl#begin> ;
                                      owl:allValuesFrom :OrganizationStatus
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://isagog.com/ontologies/top.owl#end> ;
                                      owl:allValuesFrom :OrganizationStatus
                                    ] ;
                    rdfs:comment "An organizational change" ;
                    rdfs:seeAlso <http://www.w3.org/ns/org#ChangeEvent> .


###  http://isagog.com/ontologies/organization.owl#OrganizationRole
:OrganizationRole rdf:type owl:Class ;
                  rdfs:subClassOf <http://isagog.com/ontologies/top.owl#Denotator> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://isagog.com/ontologies/top.owl#denoted> ;
                                    owl:allValuesFrom :OrganizationStatus
                                  ] ;
                  rdfs:label "Ruolo organizzativo"@it .


###  http://isagog.com/ontologies/organization.owl#OrganizationStatus
:OrganizationStatus rdf:type owl:Class ;
                    rdfs:subClassOf <http://isagog.com/ontologies/top.owl#State> .


###  http://isagog.com/ontologies/organization.owl#OrganizationUnit
:OrganizationUnit rdf:type owl:Class ;
                  rdfs:subClassOf :Organization ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :super-organization ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Organization
                                  ] ;
                  rdfs:label "Dipartimento"@it ;
                  rdfs:seeAlso <http://www.w3.org/ns/org#OrganizationUnit> .


###  http://isagog.com/ontologies/organization.owl#Participation
:Participation rdf:type owl:Class ;
               rdfs:subClassOf :OrganizationStatus ;
               rdfs:comment "Participation of an organization into another" ;
               rdfs:label "Partecipazione"@it .


