@prefix : <http://isagog.com/ontologies/knowledge#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://isagog.com/ontologies/knowledge> .

<http://isagog.com/ontologies/knowledge> rdf:type owl:Ontology ;
                                          owl:imports <http://isagog.com/ontologies/top/1.0.0> .

#################################################################
#    Object Properties
#################################################################

###  http://isagog.com/ontologies/knowledge#objectBinding
<http://isagog.com/ontologies/knowledge#objectBinding> rdfs:domain <http://isagog.com/ontologies/knowledge#RelationalSlot> ;
                                                 rdfs:range <http://isagog.com/ontologies/knowledge#ConceptualSlot> .


###  http://isagog.com/ontologies/knowledge#slotBinding
<http://isagog.com/ontologies/knowledge#slotBinding> rdfs:domain <http://isagog.com/ontologies/knowledge#Slot> ;
                                               rdfs:range <http://isagog.com/ontologies/knowledge#Slot> .


###  http://isagog.com/ontologies/knowledge#subjectBinding
<http://isagog.com/ontologies/knowledge#subjectBinding> rdfs:domain [ rdf:type owl:Class ;
                                                                owl:unionOf ( <http://isagog.com/ontologies/knowledge#AttributiveSlot>
                                                                              <http://isagog.com/ontologies/knowledge#RelationalSlot>
                                                                            )
                                                              ] ;
                                                  rdfs:range <http://isagog.com/ontologies/knowledge#Slot> .
												  
# 
# http://isagog.com/ontologies/top#predicate

:predicate a owl:DatatypeProperty ;
	rdfs:subPropertyOf <http://isagog.com/ontologies/top#identifier> ;
	rdfs:domain <http://isagog.com/ontologies/top#Denotation> ;
	rdfs:comment "Reference to a conceptual element, e.g. any owl:Class or owl.ObjectProperty instance." .											  


#################################################################
#    Data properties
#################################################################

###  http://isagog.com/ontologies/knowledge#attributeType
<http://isagog.com/ontologies/knowledge#attributeType> rdfs:domain <http://isagog.com/ontologies/knowledge#AttributiveSlot> .


###  http://isagog.com/ontologies/knowledge#isInformative
<http://isagog.com/ontologies/knowledge#isInformative> rdfs:domain <http://isagog.com/ontologies/knowledge#Slot> .


###  http://isagog.com/ontologies/knowledge#isMandatory
<http://isagog.com/ontologies/knowledge#isMandatory> rdfs:domain <http://isagog.com/ontologies/knowledge#Slot> .


#################################################################
#    Classes
#################################################################



###  http://isagog.com/ontologies/knowledge#AttributiveSlot
<http://isagog.com/ontologies/knowledge#AttributiveSlot> rdf:type owl:Class ;
                                                   rdfs:subClassOf <http://isagog.com/ontologies/knowledge#Slot> ,
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://isagog.com/ontologies/knowledge#subjectBinding> ;
                                                                     owl:someValuesFrom <http://isagog.com/ontologies/knowledge#ConceptualSlot>
                                                                   ] .


###  http://isagog.com/ontologies/knowledge#Classifier
<http://isagog.com/ontologies/knowledge#Classifier> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://isagog.com/ontologies/top#Denotation> ;
                                              rdfs:comment "Denomination of classes" .


###  http://isagog.com/ontologies/knowledge#ConceptualSlot
<http://isagog.com/ontologies/knowledge#ConceptualSlot> rdf:type owl:Class ;
                                                  rdfs:subClassOf <http://isagog.com/ontologies/knowledge#Slot> .


###  http://isagog.com/ontologies/knowledge#Frame
<http://isagog.com/ontologies/knowledge#Frame> rdf:type owl:Class ;
                                         rdfs:subClassOf <http://isagog.com/ontologies/top#Denotation> ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://isagog.com/ontologies/top#hasElement> ;
                                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                           owl:onClass <http://isagog.com/ontologies/knowledge#Slot>
                                                         ] ;
                                         rdfs:comment "Conceptual pattern, e.g. a situation or a query" .




###  http://isagog.com/ontologies/knowledge#RelationalSlot
<http://isagog.com/ontologies/knowledge#RelationalSlot> rdf:type owl:Class ;
                                                  rdfs:subClassOf <http://isagog.com/ontologies/knowledge#Slot> ,
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://isagog.com/ontologies/knowledge#objectBinding> ;
                                                                    owl:someValuesFrom <http://isagog.com/ontologies/knowledge#ConceptualSlot>
                                                                  ] ,
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://isagog.com/ontologies/knowledge#subjectBinding> ;
                                                                    owl:someValuesFrom <http://isagog.com/ontologies/knowledge#ConceptualSlot>
                                                                  ] .


###  http://isagog.com/ontologies/knowledge#Slot
<http://isagog.com/ontologies/knowledge#Slot> rdf:type owl:Class ;
                                        rdfs:subClassOf <http://isagog.com/ontologies/top#Denotation> ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty <http://isagog.com/ontologies/knowledge#predicate> ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onDataRange xsd:anyURI
                                                        ] ;
                                        rdfs:comment "Element of a conceptual pattern (frame)" .

[ rdf:type owl:Axiom ;
   owl:annotatedSource <http://isagog.com/ontologies/knowledge#Slot> ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty <http://isagog.com/ontologies/knowledge#predicate> ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:anyURI
                       ] ;
   rdfs:comment "Binding to ontology predicates, i.e. classes or properties, whose id should be in scope and typology should be consistent"
 ] .


