# #################################################################
# 
# 
# http://isagog.com/ontologies/ckg#URI

:URI a owl:DatatypeProperty ;
	rdfs:subPropertyOf :identifier ;
	rdfs:range xsd:anyURI .
# 
# http://isagog.com/ontologies/ckg#attributeType

:attributeType a owl:DatatypeProperty ;
	rdfs:subPropertyOf :feature ;
	rdfs:domain :AttributiveSlot ;
	rdfs:range _:genid7 .

_:genid7 a rdfs:Datatype ;
	owl:oneOf _:genid14 .

_:genid14 a rdf:List ;
	rdf:first "AMOUNT" ;
	rdf:rest _:genid13 .

_:genid13 a rdf:List ;
	rdf:first "IDENTIFIER" ;
	rdf:rest _:genid12 .

_:genid12 a rdf:List ;
	rdf:first "LOCATION" ;
	rdf:rest _:genid11 .

_:genid11 a rdf:List ;
	rdf:first "NUMBER" ;
	rdf:rest _:genid10 .

_:genid10 a rdf:List ;
	rdf:first "PERIOD" ;
	rdf:rest _:genid9 .

_:genid9 a rdf:List ;
	rdf:first "STRING" ;
	rdf:rest _:genid8 .

_:genid8 a rdf:List ;
	rdf:first "TEXT" ;
	rdf:rest rdf:nil .
# 
# http://isagog.com/ontologies/ckg#currency

:currency a owl:DatatypeProperty ;
	rdfs:subPropertyOf :numeral ;
	rdfs:domain :MoneyReference ;
	rdfs:range xsd:string .
# 
# http://isagog.com/ontologies/ckg#familyName

:familyName a owl:DatatypeProperty ;
	rdfs:subPropertyOf :name ;
	rdfs:domain :Person ;
	rdfs:range rdfs:Literal .
# 
# http://isagog.com/ontologies/ckg#feature

:feature a owl:DatatypeProperty .
# 
# http://isagog.com/ontologies/ckg#firstName

:firstName a owl:DatatypeProperty ;
	rdfs:subPropertyOf :name ;
	rdfs:domain :Person ;
	rdfs:range rdfs:Literal .
# 
# http://isagog.com/ontologies/ckg#form

:form a owl:DatatypeProperty ;
	rdfs:subPropertyOf :feature ;
	rdfs:domain :Word .
# 
# http://isagog.com/ontologies/ckg#identifier

:identifier a owl:DatatypeProperty ;
	rdfs:subPropertyOf owl:topDataProperty ;
	rdfs:comment "A concrete value which serves as unique identifier in some naming domain, e.g. mail address" .
# 
# http://isagog.com/ontologies/ckg#isDistinguished

:isDistinguished a owl:DatatypeProperty ;
	rdfs:subPropertyOf :feature ;
	rdfs:domain :Slot ;
	rdfs:range xsd:boolean .
# 
# http://isagog.com/ontologies/ckg#isInformative

:isInformative a owl:DatatypeProperty ;
	rdfs:subPropertyOf :feature ;
	rdfs:domain :Slot ;
	rdfs:range xsd:boolean .
# 
# http://isagog.com/ontologies/ckg#isMandatory

:isMandatory a owl:DatatypeProperty ;
	rdfs:subPropertyOf :feature ;
	rdfs:domain :Slot ;
	rdfs:range xsd:boolean .
# 
# http://isagog.com/ontologies/ckg#language

:language a owl:DatatypeProperty ;
	rdfs:subPropertyOf :feature ;
	rdfs:range _:genid15 .

_:genid15 a rdfs:Datatype ;
	owl:oneOf _:genid20 .

_:genid20 a rdf:List ;
	rdf:first "de" ;
	rdf:rest _:genid19 .

_:genid19 a rdf:List ;
	rdf:first "en" ;
	rdf:rest _:genid18 .

_:genid18 a rdf:List ;
	rdf:first "es" ;
	rdf:rest _:genid17 .

_:genid17 a rdf:List ;
	rdf:first "fr" ;
	rdf:rest _:genid16 .

_:genid16 a rdf:List ;
	rdf:first "it" ;
	rdf:rest rdf:nil .
# 
# http://isagog.com/ontologies/ckg#length

:length a owl:DatatypeProperty ;
	rdfs:subPropertyOf :textIndex .
# 
# http://isagog.com/ontologies/ckg#mailAddress

:mailAddress a owl:DatatypeProperty ;
	rdfs:subPropertyOf :identifier ;
	rdfs:range xsd:string .
# 
# http://isagog.com/ontologies/ckg#name

:name a owl:DatatypeProperty ;
	rdfs:subPropertyOf :feature .
# 
# http://isagog.com/ontologies/ckg#numeral

:numeral a owl:DatatypeProperty .
# 
# http://isagog.com/ontologies/ckg#offset

:offset a owl:DatatypeProperty ;
	rdfs:subPropertyOf :textIndex .
# 
# http://isagog.com/ontologies/ckg#partOfSpeech

:partOfSpeech a owl:DatatypeProperty ;
	rdfs:subPropertyOf :feature ;
	rdfs:domain :Word .
# 
# http://isagog.com/ontologies/ckg#predicate

:predicate a owl:DatatypeProperty ;
	rdfs:subPropertyOf :identifier ;
	rdfs:domain :Denotation ;
	rdfs:comment "Reference to a conceptual element, e.g. any owl:Class or owl.ObjectProperty instance." .
# 
# http://isagog.com/ontologies/ckg#receiverAddress

:receiverAddress a owl:DatatypeProperty ;
	rdfs:subPropertyOf :mailAddress .
# 
# http://isagog.com/ontologies/ckg#senderAddress

:senderAddress a owl:DatatypeProperty ;
	rdfs:subPropertyOf :mailAddress .
# 
# http://isagog.com/ontologies/ckg#snippet

:snippet a owl:DatatypeProperty ;
	rdfs:subPropertyOf :text ;
	rdfs:domain :Information ;
	rdfs:range xsd:string ;
	rdfs:comment "Brief summary" .
# 
# http://isagog.com/ontologies/ckg#string

:string a owl:DatatypeProperty .
# 
# http://isagog.com/ontologies/ckg#subject

:subject a owl:DatatypeProperty ;
	rdfs:subPropertyOf :title .
# 
# http://isagog.com/ontologies/ckg#text

:text a owl:DatatypeProperty ;
	rdfs:subPropertyOf :string ;
	rdfs:domain :Document ;
	rdfs:range xsd:string .
# 
# http://isagog.com/ontologies/ckg#textIndex

:textIndex a owl:DatatypeProperty ;
	rdfs:subPropertyOf :numeral ;
	rdfs:domain :TextualElement ;
	rdfs:range xsd:integer ;
	rdfs:comment "The index of a character in a text" .
# 
# http://isagog.com/ontologies/ckg#time

:time a owl:DatatypeProperty .
# 
# http://isagog.com/ontologies/ckg#timeInstant

:timeInstant a owl:DatatypeProperty ;
	rdfs:subPropertyOf :time ;
	rdfs:domain _:genid21 .

_:genid21 a owl:Class ;
	owl:unionOf _:genid23 .

_:genid23 a rdf:List ;
	rdf:first :Occurrent ;
	rdf:rest _:genid22 .

_:genid22 a rdf:List ;
	rdf:first :Period ;
	rdf:rest rdf:nil .

:timeInstant rdfs:range xsd:dateTime .
# 
# http://isagog.com/ontologies/ckg#timeSpan

:timeSpan a owl:DatatypeProperty ;
	rdfs:subPropertyOf :time ;
	rdfs:domain _:genid24 .

_:genid24 a owl:Class ;
	owl:unionOf _:genid26 .

_:genid26 a rdf:List ;
	rdf:first :Occurrent ;
	rdf:rest _:genid25 .

_:genid25 a rdf:List ;
	rdf:first :Period ;
	rdf:rest rdf:nil .

:timeSpan rdfs:range xsd:long .
# 
# http://isagog.com/ontologies/ckg#title

:title a owl:DatatypeProperty ;
	rdfs:subPropertyOf :string ;
	rdfs:domain :TextualDocument ;
	rdfs:range xsd:string .
# 
# 
# 
# #################################################################
# #
# #    Classes
# #
# #################################################################
# 
# 
# http://isagog.com/ontologies/ckg#Activity

:Activity a owl:Class ;
	rdfs:subClassOf :Event , _:genid27 .

_:genid27 a owl:Restriction ;
	owl:onProperty :involves ;
	owl:someValuesFrom :Agent .

:Activity :DOLCE "Event  depending on agents" .
# 
# http://isagog.com/ontologies/ckg#Agent

:Agent a owl:Class ;
	rdfs:subClassOf :Object ;
	rdfs:comment "Entity capable of purposely initiating actions" .
# 
# http://isagog.com/ontologies/ckg#Artefact

:Artefact a owl:Class ;
	rdfs:subClassOf :Object ;
	rdfs:comment "Entity created in a production process" .
# 
# http://isagog.com/ontologies/ckg#AttributiveSlot

:AttributiveSlot a owl:Class ;
	rdfs:subClassOf :Slot .
# 
# http://isagog.com/ontologies/ckg#Audio

:Audio a owl:Class ;
	rdfs:subClassOf :Information .
# 
# http://isagog.com/ontologies/ckg#Automata

:Automata a owl:Class ;
	rdfs:subClassOf :Agent , :Artefact ;
	rdfs:comment "Autonomous artefacts"@en .
# 
# http://isagog.com/ontologies/ckg#Classifier

:Classifier a owl:Class ;
	rdfs:subClassOf :Collection , :Denotation ;
	rdfs:comment "Denomination of classes" .
# 
# http://isagog.com/ontologies/ckg#Collection

:Collection a owl:Class ;
	rdfs:subClassOf _:genid28 .

_:genid28 a owl:Restriction ;
	owl:onProperty :hasElement ;
	owl:someValuesFrom :Entity .

:Collection rdfs:comment "Any collective thing which is cognitively and socially recognizable, whose identity does not depend on their members" ;
	rdfs:seeAlso <http://www.w3.org/2004/02/skos/core#Collection> .
# 
# http://isagog.com/ontologies/ckg#ConceptReference

:ConceptReference a owl:Class ;
	rdfs:subClassOf :Token .
# 
# http://isagog.com/ontologies/ckg#ConceptualSlot

:ConceptualSlot a owl:Class ;
	rdfs:subClassOf :Slot .
# 
# http://isagog.com/ontologies/ckg#Continuant

:Continuant a owl:Class ;
	rdfs:subClassOf :Entity ;
	:DOLCE "Endurant" ;
	rdfs:comment "Any concrete or abstract non-temporal object" .
# 
# http://isagog.com/ontologies/ckg#Dataset

:Dataset a owl:Class ;
	rdfs:subClassOf :InformationSource .
# 
# http://isagog.com/ontologies/ckg#Denotation

:Denotation a owl:Class ;
	rdfs:subClassOf :Information , _:genid29 .

_:genid29 a owl:Restriction ;
	owl:onProperty :string ;
	owl:someValuesFrom xsd:string .

:Denotation rdfs:comment "Formal description of single entities, classes or situations" .
# 
# http://isagog.com/ontologies/ckg#Dictionary

:Dictionary a owl:Class ;
	rdfs:subClassOf :Collection , _:genid30 .

_:genid30 a owl:Restriction ;
	owl:onProperty :member ;
	owl:allValuesFrom :Word .

:Dictionary rdfs:subClassOf _:genid31 .

_:genid31 a owl:Restriction ;
	owl:onProperty :language ;
	owl:minCardinality "1"^^xsd:nonNegativeInteger .
# 
# http://isagog.com/ontologies/ckg#Document

:Document a owl:Class ;
	rdfs:subClassOf :Information , _:genid32 .

_:genid32 a owl:Restriction ;
	owl:onProperty :string ;
	owl:someValuesFrom xsd:string .

:Document rdfs:comment "Textual informative entity" .

_:genid33 a owl:Axiom ;
	owl:annotatedSource :Document ;
	owl:annotatedProperty rdfs:subClassOf ;
	owl:annotatedTarget _:genid32 ;
	rdfs:comment "Textual rendering" .
# 
# http://isagog.com/ontologies/ckg#Entity

:Entity a owl:Class ;
	rdfs:comment "Any singular thing which is cognitively and socially recognizable" ;
	rdfs:seeAlso <http://www.w3.org/2004/02/skos/core#Concept> .
# 
# http://isagog.com/ontologies/ckg#Event

:Event a owl:Class ;
	rdfs:subClassOf :Occurrent ;
	:DOLCE "Occurrent featuring differentiated phases (e.g. a dinner)" .
# 
# http://isagog.com/ontologies/ckg#Feature

:Feature a owl:Class ;
	rdfs:subClassOf :Continuant , _:genid34 .

_:genid34 a owl:Restriction ;
	owl:onProperty :dependsOn ;
	owl:someValuesFrom :Object .

:Feature rdfs:comment "Identified countable dependent continuant" .
# 
# http://isagog.com/ontologies/ckg#Frame

:Frame a owl:Class ;
	rdfs:subClassOf :Denotation , _:genid35 .

_:genid35 a owl:Restriction ;
	owl:onProperty :hasElement ;
	owl:someValuesFrom :Slot .

:Frame rdfs:comment "Conceptual pattern, e.g. a situation or a query" .
# 
# http://isagog.com/ontologies/ckg#GrammarCategory

:GrammarCategory a owl:Class ;
	rdfs:subClassOf :Classifier ;
	rdfs:comment "Class of grammar categories" .
# 
# http://isagog.com/ontologies/ckg#Identifier

:Identifier a owl:Class ;
	rdfs:subClassOf :Denotation ;
	rdfs:comment "Denomination of single entities" .
# 
# http://isagog.com/ontologies/ckg#Image

:Image a owl:Class ;
	rdfs:subClassOf :Information ;
	rdfs:comment "Graphic information entity"@en .
# 
# http://isagog.com/ontologies/ckg#Information

:Information a owl:Class ;
	rdfs:subClassOf :Continuant ;
	rdfs:comment "Entities which convey meaninful information, typically by means of some medium (text, audio, etc)" .
# 
# http://isagog.com/ontologies/ckg#InformationSource

:InformationSource a owl:Class ;
	rdfs:subClassOf :Artefact , _:genid36 .

_:genid36 a owl:Restriction ;
	owl:onProperty :identifier ;
	owl:cardinality "1"^^xsd:nonNegativeInteger .

:InformationSource rdfs:comment "Entities that provide information" .
# 
# http://isagog.com/ontologies/ckg#MoneyReference

:MoneyReference a owl:Class ;
	rdfs:subClassOf :Number , :Token .
# 
# http://isagog.com/ontologies/ckg#NameReference

:NameReference a owl:Class ;
	rdfs:subClassOf :Identifier , :Token , _:genid37 .

_:genid37 a owl:Restriction ;
	owl:onProperty :string ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	owl:onDataRange xsd:string .
# 
# http://isagog.com/ontologies/ckg#Number

:Number a owl:Class ;
	rdfs:subClassOf :Information , _:genid38 .

_:genid38 a owl:Restriction ;
	owl:onProperty :numeral ;
	owl:someValuesFrom xsd:string .
# 
# http://isagog.com/ontologies/ckg#Object

:Object a owl:Class ;
	rdfs:subClassOf :Continuant ;
	rdfs:comment "Tangible countable independent entities" .
# 
# http://isagog.com/ontologies/ckg#Occurrent

:Occurrent a owl:Class ;
	rdfs:subClassOf :Entity , _:genid39 .

_:genid39 a owl:Restriction ;
	owl:onProperty :occurrentIn ;
	owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	owl:onClass :Period .

:Occurrent :DOLCE "Perdurant" ;
	rdfs:comment "Anything unfolding in time" .
# 
# http://isagog.com/ontologies/ckg#Organization

:Organization a owl:Class ;
	rdfs:subClassOf :Agent , :Collection , _:genid40 .

_:genid40 a owl:Restriction ;
	owl:onProperty :member ;
	owl:allValuesFrom :Person .

:Organization :schema "Organization" ;
	rdfs:comment "Agentive human collective" .
# 
# http://isagog.com/ontologies/ckg#OrganizationReference

:OrganizationReference a owl:Class ;
	rdfs:subClassOf :Identifier , :Token .
# 
# http://isagog.com/ontologies/ckg#Period

:Period a owl:Class ;
	rdfs:subClassOf :Continuant , _:genid41 .

_:genid41 a owl:Restriction ;
	owl:onProperty :timeInstant ;
	owl:someValuesFrom xsd:dateTime .

:Period rdfs:comment "Identified temporal span" .
# 
# http://isagog.com/ontologies/ckg#Person

:Person a owl:Class ;
	rdfs:subClassOf :Agent , _:genid42 .

_:genid42 a owl:Restriction ;
	owl:onProperty :familyName ;
	owl:someValuesFrom rdfs:Literal .

:Person rdfs:subClassOf _:genid43 .

_:genid43 a owl:Restriction ;
	owl:onProperty :firstName ;
	owl:someValuesFrom rdfs:Literal .

:Person :schema "Person" ;
	rdfs:comment "Human being"@en .
# 
# http://isagog.com/ontologies/ckg#PersonReference

:PersonReference a owl:Class ;
	rdfs:subClassOf :Identifier , :Token .
# 
# http://isagog.com/ontologies/ckg#Place

:Place a owl:Class ;
	rdfs:subClassOf :Continuant ;
	rdfs:comment "Identified spatial region" .
# 
# http://isagog.com/ontologies/ckg#RelationalSlot

:RelationalSlot a owl:Class ;
	rdfs:subClassOf :Slot .
# 
# http://isagog.com/ontologies/ckg#Sentence

:Sentence a owl:Class ;
	rdfs:subClassOf :TextualElement ;
