#################################################################
#    Classes
#################################################################

###  http://isagog.com/ontologies/descriptions#Alternative
:Alternative rdf:type owl:Class ;
             rdfs:subClassOf :Description ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :option ;
                               owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                               owl:onClass :Directive
                             ] ;
             rdfs:comment "Description of possible choices" .


###  http://isagog.com/ontologies/descriptions#Attribute
:Attribute rdf:type owl:Class ;
           rdfs:subClassOf top:Denotator ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :domain ;
                             owl:someValuesFrom :Classifier
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty top:data-value ;
                             owl:minCardinality "1"^^xsd:nonNegativeInteger
                           ] ;
           rdfs:comment "Denotation of scalar attrtibutes. Instances of this class may correspond to owl:DataProperties, via punning" .


###  http://isagog.com/ontologies/descriptions#Classifier
:Classifier rdf:type owl:Class ;
            rdfs:subClassOf top:Denotator ;
            rdfs:comment "Denotation of entities. This class reifies Entity, i.e. is a class of instances representing classes in the Entity hierarchy."@en .


###  http://isagog.com/ontologies/descriptions#Description
:Description rdf:type owl:Class ;
             rdfs:subClassOf top:Information ;
             rdfs:comment "Description of s state of affairs"@en .


###  http://isagog.com/ontologies/descriptions#Directive
:Directive rdf:type owl:Class ;
           rdfs:subClassOf :Description ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :referent ;
                             owl:allValuesFrom top:Action
                           ] ;
           rdfs:comment "Description of the execution of an action" .


###  http://isagog.com/ontologies/descriptions#Feature
:Feature rdf:type owl:Class ;
         rdfs:subClassOf :Slot ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :binding ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Attribute
                         ] ;
         rdfs:comment "A slot bound to an Attribute" .


###  http://isagog.com/ontologies/descriptions#Frame
:Frame rdf:type owl:Class ;
       rdfs:subClassOf top:Denotator ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :slot ;
                         owl:someValuesFrom :Slot
                       ] ;
       rdfs:comment "Denotation of collections of interrelated entities.In general, a frame depicts a typical situation. Specifically, here is an ontology pattern, i.e. a 'view' over an ontology, along with a 'higher-order' characterization. A frame type is a set of slots which accommodate distinguished entities (topic slots), roles (relation slots), and values (property slot). A frame instance is a network of related entities which features the pattern the frame depicts."@en .


###  http://isagog.com/ontologies/descriptions#HowManyQuestion
:HowManyQuestion rdf:type owl:Class ;
                 rdfs:subClassOf :Question .


###  http://isagog.com/ontologies/descriptions#HowMuchQuestion
:HowMuchQuestion rdf:type owl:Class ;
                 rdfs:subClassOf :Question .


###  http://isagog.com/ontologies/descriptions#HowQuestion
:HowQuestion rdf:type owl:Class ;
             rdfs:subClassOf :Question .


###  http://isagog.com/ontologies/descriptions#ProcedureDescription
:ProcedureDescription rdf:type owl:Class ;
                      rdfs:subClassOf :Description ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :referent ;
                                        owl:allValuesFrom top:Course
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :goal ;
                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass :StateDescription
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :first-step ;
                                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass [ rdf:type owl:Class ;
                                                      owl:unionOf ( :Alternative
                                                                    :Directive
                                                                  )
                                                    ]
                                      ] ;
                      rdfs:comment "Description of a course of actions and choices, e.g. a decison tree" .


###  http://isagog.com/ontologies/descriptions#Question
:Question rdf:type owl:Class ;
          rdfs:subClassOf top:Information ;
          rdfs:comment "Request of information about some entity"@en .


###  http://isagog.com/ontologies/descriptions#Relationship
:Relationship rdf:type owl:Class ;
              rdfs:subClassOf top:Denotator ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :domain ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Classifier
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :range ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Classifier
                              ] ;
              rdfs:comment "Denotation of pair of entities, insofar as they are bound each other by some conceptual link. This class reifies ObjectProperty, i.e. is the class of binary relationships which may correspond to owl ObjectProperties via 'punning'"@en .


###  http://isagog.com/ontologies/descriptions#Role
:Role rdf:type owl:Class ;
      rdfs:subClassOf :Slot ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :binding ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Relationship
                      ] ;
      rdfs:comment "A slot bound to a Relationship" .


###  http://isagog.com/ontologies/descriptions#Slot
:Slot rdf:type owl:Class ;
      rdfs:subClassOf top:Denotator ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :binding ;
                        owl:someValuesFrom top:Denotator
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :frame ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Frame
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :slot-id ;
                        owl:maxCardinality "1"^^xsd:nonNegativeInteger
                      ] ;
      rdfs:comment "Denotation of reified relationships as part of some Frame" .


###  http://isagog.com/ontologies/descriptions#StateDescription
:StateDescription rdf:type owl:Class ;
                  rdfs:subClassOf :Description ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :referent ;
                                    owl:allValuesFrom top:Occurrent
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty top:initial-time ;
                                    owl:cardinality "1"^^xsd:nonNegativeInteger
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty top:final-time ;
                                    owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                  ] ;
                  rdfs:comment "Description of a stative occurrent (state of affairs)" .


###  http://isagog.com/ontologies/descriptions#Topic
:Topic rdf:type owl:Class ;
       rdfs:subClassOf :Slot ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :binding ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Classifier
                       ] ;
       rdfs:comment "A slot bound to a Classifier" .


