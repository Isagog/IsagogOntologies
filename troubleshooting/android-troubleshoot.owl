Individual: android-troubleshoot:AppInstallationFailure

    Annotations: 
        rdfs:comment "reified class",
        top:description "è impossibile installare nuove applicazioni"@it,
        top:nominal "impossibilità di installare applicazioni"@it
    
    
Individual: android-troubleshoot:BackgroundAppCheck

    Types: 
        troubleshooting:YesNoQuestion
    
    Facts:  
     top:realization  "vi sono molte applicazioni aperte in background?"@it
    
    
Individual: android-troubleshoot:BackgroundAppPruning

    Annotations: 
        top:description "procedura per la chiusura delle applicazioni in background"@it
    
    Types: 
        troubleshooting:Guideline
    
    Facts:  
     descriptions:headline  "chiudere le applicazioni in background"@it,
     descriptions:link  "https://www.androidpit.it/bloccare-applicazioni-background"@it
    
    
Individual: android-troubleshoot:BatteryCheck

    Types: 
        troubleshooting:YesNoQuestion
    
    Facts:  
     top:realization  "la batteria si esaurisce rapidamente?"@it
    
    
Individual: android-troubleshoot:BatteryInefficiency

    Annotations: 
        top:description "la batteria si esaurisce subito"@it,
        rdfs:comment "reified class",
        top:nominal "batteria inefficiente"@it
    
    Facts:  
     troubleshooting:check  android-troubleshoot:BatteryCheck
    
    
Individual: android-troubleshoot:CheckConnectionOnMobile

    Types: 
        troubleshooting:Check
    
    Facts:  
     troubleshooting:question  "il dispositivo è connesso sulla rete mobile?"@it
    
    
Individual: android-troubleshoot:CheckConnectionOnWiFi

    Types: 
        troubleshooting:Check
    
    Facts:  
     troubleshooting:question  "il dispositivo è connesso sulla Wi-Fi?"@it
    
    
Individual: android-troubleshoot:ConnectedOnMobile

    Annotations: 
        rdfs:comment "reified class",
        top:description "Il dispositivo è connesso tramite rete mobile"@it,
        top:nominal "connessione su rete mobile"@it
    
    Types: 
        descriptions:Classifier
    
    
Individual: android-troubleshoot:ConnectedOnWiFi

    Annotations: 
        top:nominal "connessione su rete wi-fi"@it,
        top:description "il dispositivo è connesso tramite wi-fi"@it,
        rdfs:comment "reified class"
   Types: 
        descriptions:Classifier
     
    
Individual: android-troubleshoot:ConnectionLatency

    Annotations: 
        top:nominal "lentezza della connessione"@it,
        rdfs:comment "reified class",
        top:description "la connessione è lenta"@it
    Types: 
        descriptions:Classifier
    
    
Individual: android-troubleshoot:ConnectionStatusCheck

    Types: 
        troubleshooting:CheckListQuestion
    
    Facts:  
     troubleshooting:choice-1  "rete mobile"@it,
     troubleshooting:choice-2  "wi-fi"@it,
     troubleshooting:question  "su quale rete è connesso il dispositivo?"@it
    
    
Individual: android-troubleshoot:DeviceLatency

    Annotations: 
        rdfs:comment "reified class",
        top:description "Il dispositivo è lento"@it,
        top:nominal "lentezza del dispositivo"@it
   Types: 
        descriptions:Classifier
     
    
Individual: android-troubleshoot:FreeRAM

    Annotations: 
        top:description "procedura per la liberazione della RAM"
    
    Types: 
        troubleshooting:Guideline
    
    Facts:  
     descriptions:headline  "liberare la memoria ad accesso casuale (RAM)"@it
    
    
Individual: android-troubleshoot:FreeStorage

    Annotations: 
        top:description "procedura per la liberazione della memoria di massa"
    
    Types: 
        troubleshooting:Guideline
    
    Facts:  
     descriptions:headline  "liberare la memoria di massa"@it
    
    
