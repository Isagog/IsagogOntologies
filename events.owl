# http://isagog.com/ontologies/events.owl#cause

:cause a owl:ObjectProperty ;
	rdfs:subPropertyOf top:dependee ;
	owl:inverseOf :effect ;
	rdfs:domain top:Occurrent ;
	rdfs:range top:Occurrent ;
	rdfs:comment "causal relation, cause(domain,range) reads: 'range' is the cause of 'domain', e.g. cause(smoke,fire)"@en .
# 
# http://isagog.com/ontologies/events.owl#concomitant

:concomitant a owl:ObjectProperty ;
	rdfs:subPropertyOf top:temporally-related ;
	rdfs:domain top:Event ;
	rdfs:range top:Event ;
	top:ontolex-pattern "domain:Occurrent,(se,quando,mentre,nel momento in cui),range:Occurrent"@it .
# 
# http://isagog.com/ontologies/events.owl#effect

:effect a owl:ObjectProperty ;
	rdfs:subPropertyOf top:dependent ;
	rdfs:domain top:Occurrent ;
	rdfs:range top:Occurrent .
# 
# http://isagog.com/ontologies/events.owl#next

:next a owl:ObjectProperty ;
	rdfs:subPropertyOf top:temporally-related ;
	owl:inverseOf :previous .
# 
# http://isagog.com/ontologies/events.owl#previous

:previous a owl:ObjectProperty ;
