#################################################################
#    Classes
#################################################################

###  http://isagog.com/ontologies/events.owl#Achievement
:Achievement rdf:type owl:Class ;
             rdfs:subClassOf top:Change .


###  http://isagog.com/ontologies/events.owl#Cause
:Cause rdf:type owl:Class ;
       rdfs:subClassOf top:Event ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :effect ;
                         owl:someValuesFrom top:Event
                       ] ;
       rdfs:seeAlso "An event which fills the role of cause" .


###  http://isagog.com/ontologies/events.owl#Commitment
:Commitment rdf:type owl:Class ;
            rdfs:subClassOf top:State .


###  http://isagog.com/ontologies/events.owl#Failure
:Failure rdf:type owl:Class ;
         rdfs:subClassOf top:State ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :cause ;
                           owl:someValuesFrom top:Event
                         ] ;
         rdfs:comment "the inability to achieve a goal" .


###  http://isagog.com/ontologies/events.owl#Goal
:Goal rdf:type owl:Class ;
      rdfs:subClassOf top:State ;
      rdfs:comment "The final state of a telic occurrent (action)" .


###  http://isagog.com/ontologies/events.owl#Signal
:Signal rdf:type owl:Class ;
        rdfs:subClassOf top:Event ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :signalled ;
                          owl:someValuesFrom top:Event
                        ] ;
        rdfs:comment "The manifestation of some event, as a distinguished event"^^xsd:string .


