Prefix: xml: <http://www.w3.org/XML/1998/namespace>
Prefix: xsd: <http://www.w3.org/2001/XMLSchema#>



Ontology: <http://isagog.com/ontologies/android-troubleshoot.owl>

Import: <http://isagog.com/ontologies/infotech.owl>
Import: <http://isagog.com/ontologies/troubleshooting.owl>

AnnotationProperty: rdfs:comment

    
AnnotationProperty: top:description

    
AnnotationProperty: top:nominal

    
Datatype: rdf:PlainLiteral

    
ObjectProperty: events:cause

    
ObjectProperty: top:carrier

    
ObjectProperty: troubleshooting:check

    
ObjectProperty: troubleshooting:solve

    
DataProperty: descriptions:headline

    
DataProperty: descriptions:link

    
DataProperty: top:realization

    
DataProperty: troubleshooting:choice-1

    
DataProperty: troubleshooting:choice-2

    
DataProperty: troubleshooting:question

    
Class: android-troubleshoot:AndroidApp

    SubClassOf: 
        infotech:Application
    
    
Class: android-troubleshoot:AndroidDevice

    SubClassOf: 
        infotech:Mobile
    
    
Class: android-troubleshoot:AndroidOccurrent

    EquivalentTo: 
        top:carrier some android-troubleshoot:AndroidDevice
    
    SubClassOf: 
        top:Occurrent
    
    
Class: android-troubleshoot:AppInstallationFailure

    Annotations: 
        rdfs:comment "reified class",
        top:description "è impossibile installare nuove applicazioni"@it,
        top:nominal "impossibilità di installare applicazioni"@it
    
    SubClassOf: 
        android-troubleshoot:AndroidOccurrent,
        infotech:SystemFailure,
        events:cause some android-troubleshoot:StorageMemoryExausted
    
    
Class: android-troubleshoot:BatteryInefficiency

    Annotations: 
        top:description "la batteria si esaurisce subito"@it,
        rdfs:comment "reified class",
        top:nominal "batteria inefficiente"@it
    
    SubClassOf: 
        android-troubleshoot:AndroidOccurrent,
        infotech:SystemFailure,
        events:cause some 
            (android-troubleshoot:ConnectedOnMobile or android-troubleshoot:ManyRunningApps),
        troubleshooting:check value android-troubleshoot:BatteryCheck
    
    
Class: android-troubleshoot:ConnectedOnMobile

    Annotations: 
        rdfs:comment "reified class",
        top:description "Il dispositivo è connesso tramite rete mobile"@it,
        top:nominal "connessione su rete mobile"@it
    
    SubClassOf: 
        android-troubleshoot:AndroidOccurrent,
        infotech:SystemStatus,
        troubleshooting:check value android-troubleshoot:CheckConnectionOnMobile
    
    
Class: android-troubleshoot:ConnectedOnWiFi

    Annotations: 
        top:nominal "connessione su rete wi-fi"@it,
        top:description "il dispositivo è connesso tramite wi-fi"@it,
        rdfs:comment "reified class"
    
    SubClassOf: 
        android-troubleshoot:AndroidOccurrent,
        infotech:SystemStatus,
        troubleshooting:check value android-troubleshoot:CheckConnectionOnWiFi
    
    
Class: android-troubleshoot:ConnectionLatency

    Annotations: 
        top:nominal "lentezza della connessione"@it,
        rdfs:comment "reified class",
        top:description "la connessione è lenta"@it
    
    SubClassOf: 
        infotech:SystemFailure,
        events:cause some 
            (android-troubleshoot:MobileNetworkLatency or android-troubleshoot:WiFiLatency)
    
    
Class: android-troubleshoot:DeviceLatency

    Annotations: 
        rdfs:comment "reified class",
        top:description "Il dispositivo è lento"@it,
        top:nominal "lentezza del dispositivo"@it
    
    SubClassOf: 
        android-troubleshoot:AndroidOccurrent,
        infotech:SystemFailure,
        events:cause some 
            (android-troubleshoot:ManyRunningApps or android-troubleshoot:StorageMemoryExausted or (android-troubleshoot:ConnectedOnMobile
             and android-troubleshoot:MobileNetworkLatency) or (android-troubleshoot:ConnectedOnWiFi
             and android-troubleshoot:WiFiLatency))
    
    
Class: android-troubleshoot:ManyRunningApps

    Annotations: 
        rdfs:comment "reified class",
        top:description "elevato numero di applicazioni in background"@it,
        top:nominal "applicazioni in background"@it
    
    SubClassOf: 
        android-troubleshoot:AndroidOccurrent,
        infotech:SystemStatus,
        troubleshooting:check value android-troubleshoot:BackgroundAppCheck,
        troubleshooting:solve value android-troubleshoot:BackgroundAppPruning
    
    
Class: android-troubleshoot:MobileNetworkLatency

    Annotations: 
        top:nominal "lentezza della connessione su rete mobile"@it,
        rdfs:comment "reified class",
        top:description "la connessione sulla rete mobile è lenta"@it
    
    SubClassOf: 
        android-troubleshoot:AndroidOccurrent,
        infotech:SystemFailure
    
    
Class: android-troubleshoot:RamExausted

    Annotations: 
        rdfs:comment "reified class",
        top:description "la RAM è esaurita"@it,
        top:nominal "RAM insufficiente"@it
    
    SubClassOf: 
        android-troubleshoot:AndroidOccurrent,
        infotech:SystemStatus,
        events:cause some android-troubleshoot:ManyRunningApps
    
    
Class: android-troubleshoot:StorageMemoryExausted

    Annotations: 
        top:description "la memoria di massa è esaurita"@it,
        top:nominal "esaurimento della memoria di massa"@it,
        rdfs:comment "reified class"
    
    SubClassOf: 
        android-troubleshoot:AndroidOccurrent,
        infotech:SystemStatus,
        troubleshooting:check value android-troubleshoot:StorageCheck,
        troubleshooting:solve value android-troubleshoot:FreeStorage
    
    
Class: android-troubleshoot:UnexpectedAppTermination

    Annotations: 
        top:description "chiusura inattesa delle applicazioni"@it,
        rdfs:comment "reified class",
        top:nominal "chiusura inattesa delle applicazioni"@it
    
    SubClassOf: 
        infotech:SystemFailure,
        events:cause some android-troubleshoot:StorageMemoryExausted
    
    
Class: android-troubleshoot:WiFiLatency

    Annotations: 
        top:description "la connessione wi-fi è lenta"@it,
        rdfs:comment "reified class",
        top:nominal "lentezza della connessione wi-fi"@it
    
    SubClassOf: 
        android-troubleshoot:AndroidOccurrent,
        infotech:SystemFailure
    
    
Class: descriptions:Classifier

    
