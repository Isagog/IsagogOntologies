###  http://isagog.com/ontologies/documents#Document
:Document rdf:type owl:Class ;
          rdfs:subClassOf <http://isagog.com/ontologies/top#Information> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :document-section ;
                            owl:someValuesFrom :DocumentSection
                          ] ;
          rdfs:comment "Logical informative entity whose content is expressed in natural language, which can be encoded in any kind of material support" .


###  http://isagog.com/ontologies/documents#DocumentClassifier
:DocumentClassifier rdf:type owl:Class ;
                    rdfs:subClassOf <http://isagog.com/ontologies/top#Denotator> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://isagog.com/ontologies/top#denoted> ;
                                      owl:allValuesFrom :Document
                                    ] .


###  http://isagog.com/ontologies/documents#DocumentCollection
:DocumentCollection rdf:type owl:Class ;
                    rdfs:subClassOf <http://isagog.com/ontologies/top#Information> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :collection-part ;
                                      owl:allValuesFrom :Document
                                    ] .


###  http://isagog.com/ontologies/documents#DocumentSection
:DocumentSection rdf:type owl:Class ;
                 rdfs:subClassOf <http://isagog.com/ontologies/top#Information> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :main-document ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :Document
                                 ] ;
                 rdfs:comment "Informative dependent part of a document" .


