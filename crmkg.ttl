@prefix : <http://isagog.com/ontologies/crmkg#> .
@prefix ckg: <http://isagog.com/ontologies/ckg#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://isagog.com/ontologies/crmkg> .

<http://isagog.com/ontologies/crmkg> rdf:type owl:Ontology ;
                                      owl:versionIRI <http://isagog.com/ontologies/crmkg/1.0.0> ;
                                      owl:imports <http://isagog.com/ontologies/ckg/1.0.0> ;
                                      <http://purl.org/dc/elements/1.1/creator> "Guido Vetere" ;
                                      <http://purl.org/dc/terms/license> "CC-BY-SA" ;
                                      rdfs:comment "Customer Relationship Management Ontology" .

#################################################################
#    Classes
#################################################################

###  http://isagog.com/ontologies/crmkg#Address
:Address rdf:type owl:Class ;
         rdfs:subClassOf :ContactInfo ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ckg:hasPart ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :City
                         ] .


###  http://isagog.com/ontologies/crmkg#City
:City rdf:type owl:Class ;
      rdfs:subClassOf ckg:Place .


###  http://isagog.com/ontologies/crmkg#ContactInfo
:ContactInfo rdf:type owl:Class ;
             rdfs:subClassOf ckg:Information .


###  http://isagog.com/ontologies/crmkg#Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf ckg:Place .


###  http://isagog.com/ontologies/crmkg#Customer
:Customer rdf:type owl:Class ;
          rdfs:subClassOf ckg:Person ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ckg:referredBy ;
                            owl:someValuesFrom :ContactInfo
                          ] .


###  http://isagog.com/ontologies/crmkg#EmailAddress
:EmailAddress rdf:type owl:Class ;
              rdfs:subClassOf :ContactInfo .


