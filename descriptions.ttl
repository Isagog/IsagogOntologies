###  http://isagog.com/ontologies/descriptions#bind-attribute
:bind-attribute rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :binding .


###  http://isagog.com/ontologies/descriptions#bind-concept
:bind-concept rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :binding .


###  http://isagog.com/ontologies/descriptions#bind-property
:bind-property rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :binding .


###  http://isagog.com/ontologies/descriptions#binding
:binding rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf top:informative-part ;
         rdfs:domain top:Denotator ;
         rdfs:range top:Denotator ;
         rdfs:comment "Relates two denotators denoting the same entity" .


###  http://isagog.com/ontologies/descriptions#constraint
:constraint rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf top:informative-part .


###  http://isagog.com/ontologies/descriptions#domain
:domain rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :instance .


###  http://isagog.com/ontologies/descriptions#domain-topic
:domain-topic rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :constraint ;
              rdfs:domain :Slot ;
              rdfs:range :Topic .


###  http://isagog.com/ontologies/descriptions#first-step
:first-step rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf top:informative-part ;
            rdfs:domain :ProcedureDescription ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :Alternative
                                       :Directive
                                     )
                       ] .


###  http://isagog.com/ontologies/descriptions#frame
:frame rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf top:informative-whole ;
       owl:inverseOf :slot .


###  http://isagog.com/ontologies/descriptions#goal
:goal rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf top:informative-part ;
      rdfs:domain :ProcedureDescription ;
      rdfs:range :StateDescription .


###  http://isagog.com/ontologies/descriptions#instance
:instance rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf top:denoted ;
          rdfs:domain :Classifier .


###  http://isagog.com/ontologies/descriptions#next-info
:next-info rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf top:logically-related ;
           owl:inverseOf :previous-info ;
           rdfs:domain top:Sign ;
           rdfs:range top:Sign .


###  http://isagog.com/ontologies/descriptions#option
:option rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf top:logically-related ;
        rdfs:domain :Alternative ;
        rdfs:range :Directive .


###  http://isagog.com/ontologies/descriptions#option-1
:option-1 rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :option .


###  http://isagog.com/ontologies/descriptions#option-2
:option-2 rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :option .


###  http://isagog.com/ontologies/descriptions#option-3
:option-3 rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :option .


###  http://isagog.com/ontologies/descriptions#previous-info
:previous-info rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf top:logically-related ;
               rdfs:domain top:Sign ;
               rdfs:range top:Sign .


###  http://isagog.com/ontologies/descriptions#range
:range rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :instance .


###  http://isagog.com/ontologies/descriptions#range-topic
:range-topic rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :constraint ;
             rdfs:domain :Slot ;
             rdfs:range :Topic .


###  http://isagog.com/ontologies/descriptions#referent
:referent rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf top:denoted ;
          owl:inverseOf :referring ;
          rdfs:domain top:Sign .


###  http://isagog.com/ontologies/descriptions#referring
:referring rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf top:denoter ;
           rdfs:range top:Sign .


###  http://isagog.com/ontologies/descriptions#requisite
:requisite rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf top:informative-part ;
           rdfs:comment "Informative objects that are needed in order to interpret the domain's one" .


###  http://isagog.com/ontologies/descriptions#slot
:slot rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf top:element ,
                         top:informative-part ;
      rdfs:domain :Frame ;
      rdfs:range :Slot ;
      rdfs:comment "Informative part of a frame" .


###  http://isagog.com/ontologies/organization.owl#filler
<http://isagog.com/ontologies/organization.owl#filler> rdf:type owl:ObjectProperty ;
