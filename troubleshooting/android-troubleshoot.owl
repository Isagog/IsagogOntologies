    Facts:  
     troubleshooting:check  android-troubleshoot:BackgroundAppCheck,
     troubleshooting:solve  android-troubleshoot:BackgroundAppPruning
   Types: 
        descriptions:Classifier
     
    
Individual: android-troubleshoot:MobileDisconnection

    Annotations: 
