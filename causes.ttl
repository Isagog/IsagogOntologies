###  http://isagog.com/ontologies/causes#AutomatedTest
:AutomatedTest rdf:type owl:Class ;
               rdfs:subClassOf :Observation .


###  http://isagog.com/ontologies/causes#ConfidenceMeasure
:ConfidenceMeasure rdf:type owl:Class ;
                   rdfs:subClassOf <http://isagog.com/ontologies/top#Measure> ;
                   rdfs:comment "Confidence measure (how much confident is the observer)"@en .


###  http://isagog.com/ontologies/causes#EvidenceMeasure
:EvidenceMeasure rdf:type owl:Class ;
                 rdfs:subClassOf <http://isagog.com/ontologies/top#Measure> ;
                 rdfs:comment "Evidence measure (how much evidence has been gathered about the observed situation)"@en .


###  http://isagog.com/ontologies/causes#HumanObservation
:HumanObservation rdf:type owl:Class ;
                  rdfs:subClassOf :Observation .


###  http://isagog.com/ontologies/causes#Observation
:Observation rdf:type owl:Class ;
             rdfs:subClassOf <http://isagog.com/ontologies/top#Action> ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :observed ;
                               owl:someValuesFrom :Situation
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :report ;
                               owl:someValuesFrom <http://isagog.com/ontologies/top#Information>
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :evidence ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:double
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :support ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:double
                             ] ;
             owl:disjointWith :Situation .


###  http://isagog.com/ontologies/causes#ObservationFrame
:ObservationFrame rdf:type owl:Class ;
                  rdfs:subClassOf <http://isagog.com/ontologies/descriptions#Frame> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://isagog.com/ontologies/descriptions#slot> ;
                                    owl:someValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty <http://isagog.com/ontologies/infosystems.owl#bind-concept> ;
                                                                                owl:hasValue :PhysicalEntity
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty <http://isagog.com/ontologies/infosystems.owl#topic-id> ;
                                                                                owl:hasValue "entity"
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://isagog.com/ontologies/descriptions#slot> ;
                                    owl:someValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty <http://isagog.com/ontologies/infosystems.owl#bind-property> ;
                                                                                owl:hasValue :context
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty <http://isagog.com/ontologies/infosystems.owl#subject-topic> ;
                                                                                owl:hasValue "obsv"
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://isagog.com/ontologies/descriptions#slot> ;
                                    owl:someValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                owl:onProperty <http://isagog.com/ontologies/infosystems.owl#bind-property> ;
                                                                                owl:hasValue :involved
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty <http://isagog.com/ontologies/descriptions#object-topic> ;
                                                                                owl:hasValue "entity"
                                                                              ]
                                                                              [ rdf:type owl:Restriction ;
                                                                                owl:onProperty <http://isagog.com/ontologies/descriptions#subject-topic> ;
                                                                                owl:hasValue "obsv"
                                                                              ]
                                                                            ) ;
                                                         rdf:type owl:Class
                                                       ]
                                  ] ;
                  rdfs:comment "A statement which refers to an observation" .


###  http://isagog.com/ontologies/causes#Procedure
:Procedure rdf:type owl:Class ;
           rdfs:subClassOf <http://isagog.com/ontologies/top#Course> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :instruction ;
                             owl:someValuesFrom <http://isagog.com/ontologies/top#Information>
                           ] .


###  http://isagog.com/ontologies/causes#Situation
:Situation rdf:type owl:Class ;
           rdfs:subClassOf <http://isagog.com/ontologies/top#Occurrent> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :context ;
                             owl:someValuesFrom :Situation
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :fixed-by ;
                             owl:someValuesFrom :Procedure
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :observed-by ;
                             owl:someValuesFrom :Observation
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://isagog.com/ontologies/events#cause> ;
                             owl:someValuesFrom <http://isagog.com/ontologies/top#Occurrent>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://isagog.com/ontologies/top#bearer> ;
                             owl:someValuesFrom <http://isagog.com/ontologies/top#PhysicalEntity>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :evidence ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:double
                           ] .


