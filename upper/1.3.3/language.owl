###  http://isagog.com/ontologies/language.owl#Lexeme
:Lexeme rdf:type owl:Class ;
        rdfs:subClassOf top:Classifier ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :form ;
                          owl:someValuesFrom :Word
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :sense ;
                          owl:someValuesFrom :Sense
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty top:instance ;
                          owl:allValuesFrom :Word
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :base-form ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Word
                        ] .


###  http://isagog.com/ontologies/language.owl#LexicalWord
:LexicalWord rdf:type owl:Class ;
             rdfs:subClassOf :Word ,
                             [ rdf:type owl:Restriction ;
