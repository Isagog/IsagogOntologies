<http://isagog.com/ontologies/pkg> rdf:type owl:Ontology ;
                                    owl:versionIRI <http://isagog.com/ontologies/pkg/1.0.0> ;
                                    owl:imports <http://isagog.com/ontologies/ckg/1.0.0> ;
                                    dc:creator "Guido Vetere" ;
                                    dcterms:license "CC-BY Isagog Srl" ;
                                    rdfs:comment "A conceptual model for Personal Knowledge Graphs" ;
                                    rdfs:label "Personal Knowledge Graph Ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://isagog.com/ontologies/ckg#permission
ckg:permission rdf:type owl:AnnotationProperty .


###  http://isagog.com/ontologies/ckg#provenance
ckg:provenance rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#minQualifiedCardinality
owl:minQualifiedCardinality rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#qualifiedCardinality
owl:qualifiedCardinality rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://isagog.com/ontologies/ckg#elementOf
ckg:elementOf rdf:type owl:ObjectProperty .


###  http://isagog.com/ontologies/ckg#hasElement
ckg:hasElement rdf:type owl:ObjectProperty .


###  http://isagog.com/ontologies/ckg#participant
ckg:participant rdf:type owl:ObjectProperty .


###  http://isagog.com/ontologies/pkg#hasElement
:hasElement rdf:type owl:ObjectProperty .


###  http://isagog.com/ontologies/pkg#repliedBy
:repliedBy rdf:type owl:ObjectProperty ;
           owl:inverseOf :replyTo ;
           rdfs:domain :DialogElement ;
           rdfs:range :DialogElement ;
           rdfs:comment "Dialog elements relply the one another" .


###  http://isagog.com/ontologies/pkg#replyTo
:replyTo rdf:type owl:ObjectProperty ;
         rdfs:domain :DialogElement ;
         rdfs:range :DialogElement .


###  http://isagog.com/ontologies/pkg#sender
:sender rdf:type owl:ObjectProperty ;
        rdfs:domain :Email ;
        rdfs:range ckg:Agent ;
        rdfs:comment "Emails have senders (any agent)" .


#################################################################
#    Data properties
#################################################################

###  http://isagog.com/ontologies/ckg#identifier
ckg:identifier rdf:type owl:DatatypeProperty .


###  http://isagog.com/ontologies/pkg#mailAddress
:mailAddress rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf ckg:identifier ;
             rdfs:range xsd:string .


###  http://isagog.com/ontologies/pkg#receiverAddress
:receiverAddress rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :mailAddress .


###  http://isagog.com/ontologies/pkg#senderAddress
:senderAddress rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :mailAddress .


#################################################################
#    Classes
#################################################################

###  http://isagog.com/ontologies/ckg#Activity
ckg:Activity rdf:type owl:Class .


###  http://isagog.com/ontologies/ckg#Agent
ckg:Agent rdf:type owl:Class .


###  http://isagog.com/ontologies/ckg#Information
ckg:Information rdf:type owl:Class .


###  http://isagog.com/ontologies/ckg#InformationSource
ckg:InformationSource rdf:type owl:Class .


###  http://isagog.com/ontologies/ckg#Occurrent
ckg:Occurrent rdf:type owl:Class .


###  http://isagog.com/ontologies/ckg#Person
ckg:Person rdf:type owl:Class .


###  http://isagog.com/ontologies/ckg#Relationship
ckg:Relationship rdf:type owl:Class .


###  http://isagog.com/ontologies/ckg#Sentence
ckg:Sentence rdf:type owl:Class .


###  http://isagog.com/ontologies/ckg#Text
ckg:Text rdf:type owl:Class .


###  http://isagog.com/ontologies/ckg#TextualDocument
ckg:TextualDocument rdf:type owl:Class .


###  http://isagog.com/ontologies/ckg#TextualElement
ckg:TextualElement rdf:type owl:Class .


###  http://isagog.com/ontologies/pkg#Acquaintance
:Acquaintance rdf:type owl:Class ;
              rdfs:subClassOf :PersonalRelationship ;
              rdfs:comment "Personal relationship where participants are acquainted each others"@en .


###  http://isagog.com/ontologies/pkg#Conversation
:Conversation rdf:type owl:Class ;
              rdfs:subClassOf ckg:TextualDocument ,
                              :Interaction ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty ckg:hasElement ;
                                owl:someValuesFrom :DialogElement
                              ] .


###  http://isagog.com/ontologies/pkg#DialogElement
:DialogElement rdf:type owl:Class ;
               rdfs:subClassOf ckg:Text ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :repliedBy ;
                                 owl:someValuesFrom :DialogElement
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :replyTo ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :DialogElement
                               ] ;
               rdfs:comment "A conversational element" .


###  http://isagog.com/ontologies/pkg#Email
:Email rdf:type owl:Class ;
       rdfs:subClassOf ckg:TextualDocument ,
                       :DialogElement ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasElement ;
                         owl:allValuesFrom ckg:Sentence
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :repliedBy ;
                         owl:allValuesFrom :Email
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :replyTo ;
                         owl:allValuesFrom :Email
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :sender ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass ckg:Agent
                       ] ;
       rdfs:seeAlso "https://tools.ietf.org/html/rfc3864" .


###  http://isagog.com/ontologies/pkg#Interaction
:Interaction rdf:type owl:Class ;
             rdfs:subClassOf ckg:Occurrent ,
                             :PersonalRelationship ;
             rdfs:comment "Human interaction" .


###  http://isagog.com/ontologies/pkg#Meeting
:Meeting rdf:type owl:Class ;
         rdfs:subClassOf ckg:Activity ,
                         :Interaction .


###  http://isagog.com/ontologies/pkg#Message
:Message rdf:type owl:Class ;
         rdfs:subClassOf ckg:TextualElement ,
                         :DialogElement ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ckg:elementOf ;
                           owl:allValuesFrom :Conversation
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :replyTo ;
                           owl:allValuesFrom :Message
                         ] .


###  http://isagog.com/ontologies/pkg#PersonalRelationship
:PersonalRelationship rdf:type owl:Class ;
                      rdfs:subClassOf ckg:Relationship ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty ckg:participant ;
                                        owl:someValuesFrom ckg:Person
                                      ] ;
                      rdfs:comment "Relationships whose members are persons (e.g. club membership). This type of relationship does not imply personal acquaintance." .


###  http://isagog.com/ontologies/pkg#WebPage
:WebPage rdf:type owl:Class ;
         rdfs:subClassOf ckg:TextualDocument .




