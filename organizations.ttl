###  http://isagog.com/ontologies/descriptions#ProcedureDescription
desc:ProcedureDescription rdf:type owl:Class .


###  http://isagog.com/ontologies/descriptions#StateDescription
desc:StateDescription rdf:type owl:Class .


###  http://isagog.com/ontologies/organizations#ActivityType
:ActivityType rdf:type owl:Class ;
              rdfs:subClassOf top:Denotator ;
              rdfs:label "Tipo di attivita'"@it ;
              rdfs:seeAlso <https://w3id.org/italia/onto/COV#ActivityType> .


###  http://isagog.com/ontologies/organizations#Federation
:Federation rdf:type owl:Class ;
            rdfs:subClassOf :Organization ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :member-of-federation ;
                              owl:someValuesFrom :FederativeMembership
                            ] ;
            rdfs:comment "Federative organization" ;
            rdfs:label "Federazione"@it .


###  http://isagog.com/ontologies/organizations#FederativeMembership
:FederativeMembership rdf:type owl:Class ;
                      rdfs:subClassOf :OrganizationStatus ;
                      rdfs:comment "The (reified) participation of an Organization to a Federation" .


###  http://isagog.com/ontologies/organizations#LegalStatus
:LegalStatus rdf:type owl:Class ;
             rdfs:subClassOf top:Denotator ;
             rdfs:label "Forma giuridica"@it ;
             rdfs:seeAlso <https://w3id.org/italia/onto/COV#LegalStatus> .


###  http://isagog.com/ontologies/organizations#Membership
:Membership rdf:type owl:Class ;
            rdfs:subClassOf :OrganizationStatus ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty top:participant ;
                              owl:allValuesFrom top:Human
                            ] ;
            rdfs:comment "Organization membership: someone (human) participates in an organization in a certain role" .


###  http://isagog.com/ontologies/organizations#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf top:AbstractCollection ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :role-feature ;
                                owl:someValuesFrom :OrganizationRole
                              ] ;
              rdfs:comment "A collective human agent ruled by a statute, with purposes, roles and responsibilities"@en ,
                           "Associazione di persone governata da uno statuto che definisce scopi, ruoli e resposabilit√†"@it ;
              rdfs:label "Organizzazione"@it ;
              rdfs:seeAlso <http://www.w3.org/ns/org#Organization> .


###  http://isagog.com/ontologies/organizations#OrganizationChange
:OrganizationChange rdf:type owl:Class ;
                    rdfs:subClassOf desc:ProcedureDescription ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty top:begin ;
                                      owl:allValuesFrom :OrganizationStatus
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty top:end ;
                                      owl:allValuesFrom :OrganizationStatus
                                    ] ;
                    rdfs:comment "An organizational change" ;
                    rdfs:seeAlso <http://www.w3.org/ns/org#ChangeEvent> .


###  http://isagog.com/ontologies/organizations#OrganizationRole
:OrganizationRole rdf:type owl:Class ;
                  rdfs:subClassOf top:Denotator ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty top:denoted ;
                                    owl:allValuesFrom :OrganizationStatus
                                  ] ;
                  rdfs:label "Ruolo organizzativo"@it .


###  http://isagog.com/ontologies/organizations#OrganizationStatus
:OrganizationStatus rdf:type owl:Class ;
                    rdfs:subClassOf top:State .


###  http://isagog.com/ontologies/organizations#OrganizationUnit
:OrganizationUnit rdf:type owl:Class ;
                  rdfs:subClassOf :Organization ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :super-organization ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Organization
                                  ] ;
                  rdfs:label "Dipartimento"@it ;
                  rdfs:seeAlso <http://www.w3.org/ns/org#OrganizationUnit> .


###  http://isagog.com/ontologies/organizations#Participation
:Participation rdf:type owl:Class ;
               rdfs:subClassOf :OrganizationStatus ;
               rdfs:comment "Participation of an organization into another" ;
               rdfs:label "Partecipazione"@it .


